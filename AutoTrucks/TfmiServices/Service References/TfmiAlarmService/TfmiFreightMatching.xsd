          <xs:element maxOccurs="5000" name="searchIds" type="tfmi:SearchId">
            <xs:annotation>
              <xs:documentation>
                                Delete searches by their IDs. The requester must be authorized to
                                access these searches. Warnings will be returned for those searches
                                the requester is not authorized to access, but the entire request
                                will not fail.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="deleteSearchesBySearchIds" substitutionGroup="tfmi:searchDeletion" type="tfmi:DeleteSearchesBySearchIds" />
  <xs:complexType name="DeleteSearchResponse">
    <xs:complexContent mixed="false">
      <xs:extension base="message:Response">
        <xs:sequence>
          <xs:element minOccurs="0" ref="tfmi:deleteSearchResult" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="deleteSearchResponse" substitutionGroup="message:response" type="tfmi:DeleteSearchResponse" />
  <xs:complexType name="DeleteSearchResult">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Result">
        <xs:sequence>
          <xs:choice>
            <xs:element ref="tfmi:deleteSearchSuccessData" />
            <xs:element ref="types:serviceError" />
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="deleteSearchResult" substitutionGroup="types:results" type="tfmi:DeleteSearchResult" />
  <xs:complexType name="DeleteSearchSuccessData">
    <xs:complexContent mixed="false">
      <xs:extension base="types:SuccessData" />
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="deleteSearchSuccessData" substitutionGroup="types:data" type="tfmi:DeleteSearchSuccessData" />
  <xs:complexType name="LookupSearchMatchesRequest">
    <xs:annotation>
      <xs:documentation>
                Retrieves additional matches not included in the response to a prior 
                CreateSearchRequest. 
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="message:Request">
        <xs:sequence>
          <xs:element ref="tfmi:lookupSearchMatchesOperation" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupSearchMatchesRequest" substitutionGroup="message:request" type="tfmi:LookupSearchMatchesRequest" />
  <xs:complexType name="LookupSearchMatchesOperation">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Operation">
        <xs:sequence>
          <xs:element name="searchId" type="tfmi:SearchId">
            <xs:annotation>
              <xs:documentation>
                                The ID of the search which provided the supplied list of matching 
                                asset IDs. Any IDs in the list which were not supplied (i.e., 
                                matched by) this search will be silently rejected.<br /><br />
                                This search must be owned by the requesting user.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="3000" name="matchingIds" type="tfmi:AssetId">
            <xs:annotation>
              <xs:documentation>
                                List of IDs of matching assets to lookup. This is typically a
                                pass-around of CreateSearchSuccessData.remainingMatchingIds or an
                                immediately-previous
                                LookupSearchMatchesSuccessData.remainingMatchingIds.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupSearchMatchesOperation" substitutionGroup="types:operations" type="tfmi:LookupSearchMatchesOperation" />
  <xs:complexType name="LookupSearchMatchesResponse">
    <xs:complexContent mixed="false">
      <xs:extension base="message:Response">
        <xs:sequence>
          <xs:element ref="tfmi:lookupSearchMatchesResult" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupSearchMatchesResponse" substitutionGroup="message:response" type="tfmi:LookupSearchMatchesResponse" />
  <xs:complexType name="LookupSearchMatchesResult">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Result">
        <xs:choice>
          <xs:element ref="tfmi:lookupSearchMatchesSuccessData" />
          <xs:element ref="types:serviceError" />
        </xs:choice>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupSearchMatchesResult" substitutionGroup="types:results" type="tfmi:LookupSearchMatchesResult" />
  <xs:complexType name="LookupSearchMatchesSuccessData">
    <xs:complexContent mixed="false">
      <xs:extension base="types:SuccessData">
        <xs:sequence>
          <xs:element name="searchId" type="tfmi:SearchId">
            <xs:annotation>
              <xs:documentation>
                                ID of the search whose results were looked up (an echo-back of
                                LookupSearchMatchesOperation.searchId).
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="300" name="matches" type="tfmi:MatchingAsset">
            <xs:annotation>
              <xs:documentation>
                                Detailed information on matching assets specified in
                                LookupSearchMatchesOperation.matchingIds.<br /><br />
                                The number of detailed matches returned is limited to that
                                specified in LookupSearchMatchesOperation.maxDetailedMatches.<br /><br />
                                The detailed match information is supplied only for those matching
                                asset IDs that are currently active at the time the
                                LookupSearchMatchesRequest is processed. Assets from the matching
                                ID list are checked until either enough are found to fulfill the
                                specified maximum number of matches, or the list is exhausted. It
                                is possible that no currently-active matching assets will be 
                                found.<br /><br />
                                Therefore, if the client is always faithful to pass in an ID list
                                resulting from CreateSearchSuccessData.matchingsIds or
                                LookupSearchMatchesSucessData.remainingMatchingIds, it is 
                                guaranteed to always get back detailed match information on active
                                assets in the sort order specified by the originating 
                                CreateSearchRequest. The client is, of course, free to tamper with
                                the order of the IDs sent in on the LookupSearchMatchesRequest
                                (and the resulting detailed match information will be in the same
                                order as the IDs list).
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="remainingMatchingIds" type="tfmi:AssetId">
            <xs:annotation>
              <xs:documentation>
                                This is a proper subset of the list of IDs of matching assets
                                supplied in the LookupSearchMatchesRequest, less those returned
                                in element "detailedMatches" and less those that are now inactive or
                                cannot be found or otherwise cannot be accessed by the requester. 
                                This provides the client with a pared-down list of matching IDs 
                                that can be submitted on subsequent LookupSearchMatchesRequest(s). 
                                It is possible that there will be no remaining matching assetIDs.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupSearchMatchesSuccessData" substitutionGroup="types:data" type="tfmi:LookupSearchMatchesSuccessData" />
  <xs:complexType name="CountAssetsRequest">
    <xs:annotation>
      <xs:documentation>
                The CountAssets request is functionally identical to the CreateSearch request,
                with two important differences:
                <ol><li>The response contains only a count of matching assets, and does
                        not include details on any of the matches.</li><li>The search record is not stored, so you cannot subsequently
                        lookup or delete the search (i.e., there are no analogues to
                        LookupSearchRequest or DeleteSearchRequest).</li></ol>
                CountAssetsRequest is expected to be used for research and analytic activities, 
                rather than active freight-matching. High search volumes are normal, but the 
                total search volume will generally be limited on a daily, weekly, or monthly basis 
                by subscription levels. Please contact your salesperson if you need to increase 
                your subscription level to raise your search quotas.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="message:Request">
        <xs:sequence>
          <xs:element ref="tfmi:countAssetsOperation" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="countAssetsRequest" substitutionGroup="message:request" type="tfmi:CountAssetsRequest" />
  <xs:complexType name="CountAssetsOperation">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Operation">
        <xs:sequence>
          <xs:element name="criteria" type="tfmi:SearchCriteria">
            <xs:annotation>
              <xs:documentation>
                                The criteria to be used for this search.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="countAssetsOperation" substitutionGroup="types:operations" type="tfmi:CountAssetsOperation" />
  <xs:complexType name="CountAssetsResponse">
    <xs:complexContent mixed="false">
      <xs:extension base="message:Response">
        <xs:sequence>
          <xs:element minOccurs="0" ref="tfmi:countAssetsResult" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="countAssetsResponse" substitutionGroup="message:response" type="tfmi:CountAssetsResponse" />
  <xs:complexType name="CountAssetsResult">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Result">
        <xs:choice>
          <xs:element ref="tfmi:countAssetsSuccessData" />
          <xs:element ref="types:serviceError" />
        </xs:choice>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="countAssetsResult" substitutionGroup="types:results" type="tfmi:CountAssetsResult" />
  <xs:complexType name="CountAssetsSuccessData">
    <xs:complexContent mixed="false">
      <xs:extension base="types:SuccessData">
        <xs:sequence>
          <xs:element name="count" type="types:NonNegativeInt">
            <xs:annotation>
              <xs:documentation>
                                The total number of matches found.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="countAssetsSuccessData" substitutionGroup="types:data" type="tfmi:CountAssetsSuccessData" />
  <xs:complexType name="LookupCarrierInfoRequest">
    <xs:annotation>
      <xs:documentation>
                Lookup information about a carrier.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="message:Request">
        <xs:sequence>
          <xs:element ref="tfmi:lookupCarrierInfoOperation" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCarrierInfoRequest" substitutionGroup="message:request" type="tfmi:LookupCarrierInfoRequest" />
  <xs:complexType name="LookupCarrierInfoOperation">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Operation">
        <xs:sequence>
          <xs:element minOccurs="0" name="version" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>
                                Deprecated and unused.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice>
            <xs:element name="userId" type="types:UserId">
              <xs:annotation>
                <xs:documentation>
                                    The userId of a user belonging to the requested carrier.
                                    This UserId will normally be obtained from a matching asset
                                    returned from a search or alarm match.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="mcNumber" type="types:NonNegativeInt">
              <xs:annotation>
                <xs:documentation>
                                    The MC Number of the requested carrier.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="dotNumber" type="types:NonNegativeInt">
              <xs:annotation>
                <xs:documentation>
                                    The DOT Number of the requested carrier.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="includeDotAuthority" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include DOT Authority
                                information.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeDotInsurance" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include DOT Active / Pending 
                                insurance information.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeFmcsaSafetyRating" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include FMCSA Safety
                                information.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeFmcsaCrashInspection" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include FMCSA Crash
                                and inspection information.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeFmcsaSafeStat" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include FMCSA SafeStat
                                information.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeCsa2010SafetyFitness" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include CSA 2010 safety
                                fitness data.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeCsa2010Basic" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include CSA 2010 basic
                                measurement data.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeDotProfile" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include
                                the
                                carrier's
                                DOT Profile.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeExtendedProfile" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include the 'extended' 
                                profile information maintained by TransCore.<br /></xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCarrierInfoOperation" substitutionGroup="types:operations" type="tfmi:LookupCarrierInfoOperation" />
  <xs:complexType name="LookupCarrierInfoResponse">
    <xs:complexContent mixed="false">
      <xs:extension base="message:Response">
        <xs:sequence>
          <xs:element minOccurs="0" ref="tfmi:lookupCarrierInfoResult" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCarrierInfoResponse" substitutionGroup="message:response" type="tfmi:LookupCarrierInfoResponse" />
  <xs:complexType name="LookupCarrierInfoResult">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Result">
        <xs:sequence>
          <xs:choice>
            <xs:element ref="tfmi:lookupCarrierInfoSuccessData" />
            <xs:element ref="types:serviceError" />
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCarrierInfoResult" substitutionGroup="types:results" type="tfmi:LookupCarrierInfoResult" />
  <xs:complexType name="LookupCarrierInfoSuccessData">
    <xs:complexContent mixed="false">
      <xs:extension base="types:SuccessData">
        <xs:sequence>
          <xs:element minOccurs="0" name="dotHeader" type="tfmi:DotHeader">
            <xs:annotation>
              <xs:documentation>
                                Basic information maintained by DOT about the carrier.<br /><br />
                                TransCore does not have DOT information on all carriers, so this
                                element will occasionally be omitted.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="dotAuthority" type="tfmi:DotAuthority">
            <xs:annotation>
              <xs:documentation>
                                Describes the common, contract and broker authority status of the
                                target carrier.<br /><br />
                                Included only if the 'includeDotAuthority' flag is specified and if
                                the carrier is currently authorized. If the 'includeDotAuthority'
                                flag was specified and this element is <i>not</i> included in the 
                                response, then the carrier is <i>not</i> authorized.<br /><br /></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="dotInsurance">
            <xs:annotation>
              <xs:documentation>
                                Describes a type of insurance carried by this carrier. An element
                                will be included for each of the insurance types carried by the 
                                carrier (possibly up to 5 types).<br /><br />
                                Included only if the 'includeDotInsurance' flag is specified.<br /><br />
                                TransCore does not have DOT information on all carriers, so this
                                element will occasionally be omitted even if the
                                'includeDotInsurance' flag was specified. This is generally the
                                case for carriers without authorization (see dotAuthority), but
                                occasionaly true even for authorized carriers.<br /><br />
                                Describes the mandated insurance coverage filed with the FMSCA.
                                This will include Primary BIPD insurance for all carriers (Form
                                BMC-91). It may include excess BIPD insurance if applicable
                                (Form BMC-91X). Common carriers are also required to file evidence
                                of cargo coverage (Form BMC-34). If the entity has brokerage 
                                authority, this section will also include details of the broker
                                bond (Form BMC-84) or trust fund (Form BMC-85).<br /><br />
                                BIPD: Bodily Injury and Personal Damage, required for all 
                                carriers.<br />
                                Cargo: Cargo coverage, required for common carriers.<br />
                                Bond: Broker bond.<br />
                                Trust: Trust fund.<br /><br />
                                Brokers must provide evidence of a bond or trust fund. Either is
                                acceptable. Each element will only be present if the target
                                carrier also holds brokerage authority under the same MC (docket
                                number).
                            </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" name="bipd" type="tfmi:InsuranceDescription" />
                <xs:element minOccurs="0" name="excess" type="tfmi:InsuranceDescription" />
                <xs:element minOccurs="0" name="cargo" type="tfmi:InsuranceDescription" />
                <xs:element minOccurs="0" name="bond" type="tfmi:InsuranceDescription" />
                <xs:element minOccurs="0" name="trust" type="tfmi:InsuranceDescription" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="fmcsaSafetyRating" type="tfmi:FmcsaSafetyRating">
            <xs:annotation>
              <xs:documentation>
                                Describes FMCSA-maintained safety data.<br /><br />
                                Included only if the 'includeFmcsaSafetyRating' flag is
                                specified
                                and
                                if TransCore has current FMCSA Safety data on the
                                specified
                                company.<br /><br />
                                Describes the carrier's most recent safety rating from the
                                FMCSA.
                                For
                                more information regarding the source of this
                                information
                                go to
                                http://safer.fmcsa.dot.gov/saferhelp.aspx#SafetyRating
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="fmcsaCrashInspection" type="tfmi:FmcsaCrashInspection">
            <xs:annotation>
              <xs:documentation>
                                Describes FMCSA-maintained crash and inspection data.<br /><br />
                                Included only if the 'includeFmcsaCrashInspection' flag is
                                specified
                                and if TransCore has current FMCSA Crash/Inspection
                                data on the
                                specified company.<br /><br />
                                Describes the carrier's crash and inspection record over the
                                last
                                24
                                months. For more information regarding the source of this
                                information go to
                                http://safer.fmcsa.dot.gov/saferhelp.aspx#Inspection
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="fmcsaSafeStat" type="tfmi:FmcsaSafeStat">
            <xs:annotation>
              <xs:documentation>
                                Describes FMCSA-maintained safety data.<br /><br />
                                Included only if the 'includeFmcsaSafeStat' flag is specified
                                and if
                                TransCore has current FMCSA SafeStat data on the
                                specified company.<br /><br />
                                Describes the carrier's most recent safety evaluations scores
                                from
                                the FMCSA SafeStat system. For more information regarding
                                the source
                                of this information go to
                                http://ai.fmcsa.dot.gov/SafeStat/SafeStatMain.asp.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="dotProfile" type="tfmi:DotProfile">
            <xs:annotation>
              <xs:documentation>
                                Describes the DOT-maintained information about the carrier,
                                including the equipment owned, drivers employed, and commodities
                                carried.<br /><br />
                                Included only if the 'includeDotProfile' flag is specified.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCarrierInfoSuccessData" substitutionGroup="types:data" type="tfmi:LookupCarrierInfoSuccessData" />
  <xs:complexType name="LookupCarrierRequest">
    <xs:annotation>
      <xs:documentation>
                Lookup insurance and DOT information about a
                carrier from TransCore's
                CarrierWatch system.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="message:Request">
        <xs:sequence>
          <xs:element maxOccurs="50" ref="tfmi:lookupCarrierOperation" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCarrierRequest" substitutionGroup="message:request" type="tfmi:LookupCarrierRequest" />
  <xs:complexType name="LookupCarrierOperation">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Operation">
        <xs:sequence>
          <xs:element minOccurs="0" name="version" type="xs:decimal">
            <xs:annotation>
              <xs:documentation>
                                Deprecated and no longer used.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice>
            <xs:element name="userId" type="types:UserId">
              <xs:annotation>
                <xs:documentation>
                                    The UserId of a user belonging to the requested carrier.
                                    This UserId will normally be obtained from a matching asset
                                    returned from a search or alarm match.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="docket" type="cwtypes:DocketType">
              <xs:annotation>
                <xs:documentation>
                                    The docket prefix and number of the requested carrier (e.g. 
                                    MC number, FF number).
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="intrastate" type="cwtypes:IntrastateType">
              <xs:annotation>
                <xs:documentation>
                                    The instrastate code and state of the requested carrier.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="mcNumber" type="types:NonNegativeInt">
              <xs:annotation>
                <xs:documentation>
                                    The MC Number of the requested carrier. Note that MC number 
                                    lookup is also available through the <code>docket</code> 
                                    option. This option is retained for ease of use and backwards 
                                    compatibility. 
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="dotNumber" type="types:NonNegativeInt">
              <xs:annotation>
                <xs:documentation>
                                    The DOT Number of the requested carrier.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="includeDotAuthority" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include DOT Authority
                                information.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeDotInsurance" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include DOT Active /
                                Pending insurance information.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeInsuranceCertificates" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include the company's
                                insurance certificate information maintained by TransCore.<br /><br />
                                Note: Insurance certificate information is only available with 
                                premium CarrierWatch subscriptions. If you wish to upgrade, please 
                                contact your account representative.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeFmcsaSafetyRating" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include FMCSA Safety
                                information.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeFmcsaInspections" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include FMCSA inspection
                                information.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeFmcsaCrashes" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include FMCSA crash
                                information.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeFmcsaSafeStat" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include FMCSA SafeStat
                                information.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeCsa2010SafetyFitness" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include CSA 2010 safety
                                fitness data.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeCsa2010Basic" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include CSA 2010 basic
                                measurement data.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeDotProfile" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include the carrier's
                                DOT Profile.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="includeExtendedProfile" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Specifies whether the response should include the 'extended'
                                profile information maintained by TransCore.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCarrierOperation" substitutionGroup="types:operations" type="tfmi:LookupCarrierOperation" />
  <xs:complexType name="LookupCarrierResponse">
    <xs:complexContent mixed="false">
      <xs:extension base="message:Response">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="50" ref="tfmi:lookupCarrierResult" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCarrierResponse" substitutionGroup="message:response" type="tfmi:LookupCarrierResponse" />
  <xs:complexType name="LookupCarrierResult">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Result">
        <xs:sequence>
          <xs:choice>
            <xs:element ref="tfmi:lookupCarrierSuccessData" />
            <xs:element ref="types:serviceError" />
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCarrierResult" substitutionGroup="types:results" type="tfmi:LookupCarrierResult" />
  <xs:complexType name="LookupCarrierSuccessData">
    <xs:complexContent mixed="false">
      <xs:extension base="types:SuccessData">
        <xs:sequence>
          <xs:element minOccurs="0" name="header" type="cwtypes:Header">
            <xs:annotation>
              <xs:documentation>
                                Basic information maintained by DOT about the carrier. Note that
                                there are (hopefully rare) cases in which the DOT does not provide
                                TransCore with even this basic information, in which case this
                                element will be omitted.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="dotProfile" type="cwtypes:DotProfile">
            <xs:annotation>
              <xs:documentation>
                                Provides the operation details of a carrier available at DOT.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="dotAuthority" type="cwtypes:DotAuthority">
            <xs:annotation>
              <xs:documentation>
                                Included only if the 'includeDotAuthority' flag is specified.<br /><br />
                                This section describes the common, contract and broker authority
                                status of the target carrier.<br /><br />
                                If the 'includeDotAuthority' flag was specified and this element is
                                <i>not</i> included in the response, then the carrier is <i>not</i>
                                authorized.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="dotInsurance" type="cwtypes:DotInsuranceType">
            <xs:annotation>
              <xs:documentation>
                                Describes a type of insurance carried by this carrier. An element
                                will be included for each of the insurance types carried by the
                                carrier.<br /><br />
                                Included only if the 'includeDotInsurance' flag is specified.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice minOccurs="0">
            <xs:annotation>
              <xs:documentation>
                                If private insurance information was requested (see
                                includeInsuranceCertificates), the insurance certificates will
                                be returned here. In two cases, a serviceError will be returned
                                instead:<br /><br />
                                1) The requesting user does not have a subscription that allows
                                lookup of private insurance information.
                                2) TransCore does not have an insurance certificate on file for
                                the requested carrier. This case does not imply anything positive
                                or negative regarding the carrier's fitness.<br /><br />
                                Included only if the includeInsuranceCertificates flag is
                                specified and if the requesting user has a subscription which
                                includes access to insurance certificates.
                            </xs:documentation>
            </xs:annotation>
            <xs:element minOccurs="0" name="insuranceCertificates" type="cwtypes:InsuranceCertificates">
              <xs:annotation>
                <xs:documentation>
                                    Provides details on each insurance certificate carried by the 
                                    carrier.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="insuranceCertificateWarning" type="types:ServiceError">
              <xs:annotation>
                <xs:documentation>
                                    Returned if no insurance certificates were on file for the 
                                    requested carrier. The lack of insurance certificates should 
                                    not be interpreted as a negative mark on the carrier. To 
                                    request that TransCore begin monitoring insurance on a carrier,
                                    see MonitorInsuranceRequest.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element minOccurs="0" name="safestat" type="cwtypes:FmcsaSafeStat">
            <xs:annotation>
              <xs:documentation>
                                Describes FMCSA-maintained safety data.<br /><br />
                                Included only if the 'includeSafestat' flag is specified and if
                                TransCore has current FMCSA SafeStat data on the specified 
                                company.<br /><br />
                                Describes the carrier's most recent safety evaluations scores from
                                the FMCSA SafeStat system. For more information regarding the 
                                source of this information go to
                                http://ai.fmcsa.dot.gov/SafeStat/SafeStatMain.asp.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="safetyRating" type="cwtypes:FmcsaSafetyRating">
            <xs:annotation>
              <xs:documentation>
                                Describes FMCSA-maintained safety data.<br /><br />
                                Included only if the 'includeSafetyRating' flag is specified and 
                                if TransCore has current FMCSA Safety data on the specified 
                                company.<br /><br />
                                Describes the carrier's most recent safety rating from the FMCSA.
                                For more information regarding the source of this information
                                go to http://safer.fmcsa.dot.gov/saferhelp.aspx#SafetyRating<br /></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="fmcsaInspections" type="cwtypes:FmcsaInspections">
            <xs:annotation>
              <xs:documentation>
                                Describes FMCSA-maintained crash and inspection data.<br /><br />
                                Included only if the 'includeFmcsaCrashInspection' flag is specified
                                and if TransCore has current FMCSA Crash/Inspection data on the
                                specified company.<br /><br />
                                Describes the carrier's crash and inspection record over the last
                                24 months. For more information regarding the source of this 
                                information go to
                                http://safer.fmcsa.dot.gov/saferhelp.aspx#Inspection
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="fmcsaCrashes" type="cwtypes:FmcsaCrashes">
            <xs:annotation>
              <xs:documentation>
                                Describes FMCSA-maintained crash and inspection data.<br /><br />
                                Included only if the 'includeFmcsaCrashInspection' flag is 
                                specified and if TransCore has current FMCSA Crash/Inspection
                                data on the specified company.<br /><br />
                                Describes the carrier's crash and inspection record over the last
                                24 months. For more information regarding the source of this
                                information go to
                                http://safer.fmcsa.dot.gov/saferhelp.aspx#Inspection
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="csa2010SafetyFitness" type="cwtypes:Csa2010SafetyFitness">
            <xs:annotation>
              <xs:documentation>
                                CSA 2010 Safety Fitness data.<br /><br />
                                Included only if the <code>includeCsa2010SafetyFitness</code> flag 
                                is specified and if TransCore has current CSA 2010 data on the
                                specified company.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="csa2010Basics" type="cwtypes:Csa2010BasicMeasurements">
            <xs:annotation>
              <xs:documentation>
                                CSA 2010 Basic Measurement data.<br /><br />
                                Included only if the <code>includeCsa2010Basic</code> flag is 
                                specified and if TransCore has current CSA 2010 data on the 
                                specified company.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="extendedProfile" type="cwtypes:ExtendedProfile">
            <xs:annotation>
              <xs:documentation>
                                Additional data collected from the carrier by TransCore.<br /><br />
                                Included only if the <code>includeExtendedProfile</code> flag is 
                                specified.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCarrierSuccessData" substitutionGroup="types:data" type="tfmi:LookupCarrierSuccessData" />
  <xs:complexType name="MonitorInsuranceRequest">
    <xs:annotation>
      <xs:documentation>
                Request that a carrier in the TransCore CarrierWatch system be monitored for 
                insurance information.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="message:Request">
        <xs:sequence>
          <xs:element ref="tfmi:monitorInsuranceOperation" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="monitorInsuranceRequest" substitutionGroup="message:request" type="tfmi:MonitorInsuranceRequest" />
  <xs:complexType name="MonitorInsuranceOperation">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Operation">
        <xs:sequence>
          <xs:element name="requesterName" type="types:String50">
            <xs:annotation>
              <xs:documentation>
                                Requester's name.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="requesterPhoneNumber" type="types:PhoneNumber">
            <xs:annotation>
              <xs:documentation>
                                Requester's phone number.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="requesterVendorId" type="types:NonNegativeInt">
            <xs:annotation>
              <xs:documentation>
                                The ID that the TMS uses for the carrier. Transcore's integrated 
                                customers have their own ID system for carriers and pass those IDs 
                                to Transcore to map Transcore's data to the desired entity in the 
                                TMS. <br /><br />
                                The TMS might be able to grab this automatically based on context.
                                However, if Transcore have not previously received the Vendor ID,
                                it might not be of use on Transcore's side.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="loadingToday" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Indicates a desire or plan to load the carrier today. If specified,
                                this is considered a "Hot Request" with higher priority and
                                shorter turn-around time (premium subscriptions only). If omitted 
                                or false, this request is treated as standard priority with 
                                approximately 48-hour turnaround.<br /><br />
                                NOTE: This is designed only to be used for urgent situations, 
                                never as the default method for adding a carrier. If you plan 
                                to leverage this feature, you MUST discuss how you will utilize it 
                                in your TMS with TransCore FIRST before beginning your 
                                implementation.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="carrierDotNumber" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation>
                                DOT Number of the carrier.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="carrierDocketNumber" type="cwtypes:DocketType">
            <xs:annotation>
              <xs:documentation>
                                Docket number of the carrier (e.g., MC, FF number).
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="carrierIntrastateCode" type="cwtypes:IntrastateType">
            <xs:annotation>
              <xs:documentation>
                                Intrastate identifier of the carrier.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="comments" type="types:String512">
            <xs:annotation>
              <xs:documentation>
                                Any additional pertinent information for the TransCore 
                                representative.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="monitorInsuranceOperation" substitutionGroup="types:operations" type="tfmi:MonitorInsuranceOperation" />
  <xs:complexType name="MonitorInsuranceResponse">
    <xs:complexContent mixed="false">
      <xs:extension base="message:Response">
        <xs:sequence>
          <xs:element ref="tfmi:monitorInsuranceResult" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="monitorInsuranceResponse" substitutionGroup="message:response" type="tfmi:MonitorInsuranceResponse" />
  <xs:complexType name="MonitorInsuranceResult">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Result">
        <xs:sequence>
          <xs:choice>
            <xs:element ref="tfmi:monitorInsuranceSuccessData" />
            <xs:element ref="types:serviceError" />
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="monitorInsuranceResult" substitutionGroup="types:results" type="tfmi:MonitorInsuranceResult" />
  <xs:complexType name="MonitorInsuranceSuccessData">
    <xs:complexContent mixed="false">
      <xs:extension base="types:SuccessData" />
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="monitorInsuranceSuccessData" substitutionGroup="types:data" type="tfmi:MonitorInsuranceSuccessData" />
  <xs:complexType name="CreateCarrierRequest">
    <xs:annotation>
      <xs:documentation>
                Request that a carrier not yet in the TransCore CarrierWatch system be added 
                for the purposes of monitoring DOT and insurance information. TransCore already 
                monitors DOT information on the vast majority of interstate carriers, so this 
                request is primarily useful for intra-state carriers. If the requester is a 
                premium CarrierWatch customer (with the ability to lookup private insurance 
                information), TransCore will additionally begin monitoring private insurance 
                information on the specified carrier (see also MonitorInsuranceRequest).
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="message:Request">
        <xs:sequence>
          <xs:element ref="tfmi:createCarrierOperation" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="createCarrierRequest" substitutionGroup="message:request" type="tfmi:CreateCarrierRequest" />
  <xs:complexType name="CreateCarrierOperation">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Operation">
        <xs:sequence>
          <xs:element name="requesterName" type="types:String50">
            <xs:annotation>
              <xs:documentation>
                                Requester's name.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="requesterPhoneNumber" type="types:PhoneNumber">
            <xs:annotation>
              <xs:documentation>
                                Requester's phone number.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="requesterVendorId" type="types:NonNegativeInt">
            <xs:annotation>
              <xs:documentation>
                                The ID that the TMS uses for the carrier. Transcore's integrated
                                customers have their own ID system for carriers and pass those IDs 
                                to Transcore to map Transcore's data to the right entity in the 
                                TMS.<br /><br />
                                The TMS might be able to grab this automatically based on context.
                                However, if Transcore have not previously received the Vendor ID,
                                it might not be of use on Transcore's side.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="loadingToday" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Indicates a desire or plan to load the carrier today. If specified,
                                this is considered a "Hot Request" with higher priority and
                                shorter turn-around time (premium subscriptions only). If omitted 
                                or false, this request is treated as standard priority with 
                                approximately 48-hour turnaround.<br /><br />
                                NOTE: This is designed only to be used for urgent situations, 
                                never as the default method for adding a carrier. If you plan 
                                to leverage this feature, you MUST discuss how you will utilize it 
                                in your TMS with TransCore FIRST before beginning your 
                                implementation.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="carrierCompanyName" type="types:String50">
            <xs:annotation>
              <xs:documentation>
                                Carrier's name.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="carrierAddressLine1" type="types:String50">
            <xs:annotation>
              <xs:documentation>
                                Carrier's address.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="carrierAddressLine2" type="types:String50">
            <xs:annotation>
              <xs:documentation>
                                Carrier's address details.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="carrierCity" type="types:String50">
            <xs:annotation>
              <xs:documentation>
                                Carrier's city.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="carrierState" type="types:StateProvince">
            <xs:annotation>
              <xs:documentation>
                                Carrier's state.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="carrierZipCode" type="types:PostalCodePattern">
            <xs:annotation>
              <xs:documentation>
                                Carrier's zip/postal code.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="carrierDotNumber" type="xs:nonNegativeInteger">
            <xs:annotation>
              <xs:documentation>
                                Carrier's DOT number.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="carrierDocketNumber" type="cwtypes:DocketType">
            <xs:annotation>
              <xs:documentation>
                                Please enter at least one of the following, so TransCore can 
                                identify the carrier:
                                <ul><li>carrierMcNumber</li><li>carrierIntrastateCode</li><li>carrierDotNumber.</li></ul></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="carrierIntrastateCode" type="cwtypes:IntrastateType">
            <xs:annotation>
              <xs:documentation>
                                Carrier's intrastate identifier.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="carrierContactName" type="types:String50">
            <xs:annotation>
              <xs:documentation>
                                Primary contact at the carrier.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="carrierPhoneNumber" type="types:PhoneNumber">
            <xs:annotation>
              <xs:documentation>
                                Carrier's contact phone number.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="carrierFaxNumber" type="types:PhoneNumber">
            <xs:annotation>
              <xs:documentation>
                                Carrier's contact fax number.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="comments" type="types:String512">
            <xs:annotation>
              <xs:documentation>
                                Any additional pertinent information for the TransCore 
                                representative.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="createCarrierOperation" substitutionGroup="types:operations" type="tfmi:CreateCarrierOperation" />
  <xs:complexType name="CreateCarrierResponse">
    <xs:complexContent mixed="false">
      <xs:extension base="message:Response">
        <xs:sequence>
          <xs:element minOccurs="0" ref="tfmi:createCarrierResult" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="createCarrierResponse" substitutionGroup="message:response" type="tfmi:CreateCarrierResponse" />
  <xs:complexType name="CreateCarrierResult">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Result">
        <xs:sequence>
          <xs:choice>
            <xs:element ref="tfmi:createCarrierSuccessData" />
            <xs:element ref="types:serviceError" />
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="createCarrierResult" substitutionGroup="types:results" type="tfmi:CreateCarrierResult" />
  <xs:complexType name="CreateCarrierSuccessData">
    <xs:complexContent mixed="false">
      <xs:extension base="types:SuccessData">
        <xs:sequence>
          <xs:element minOccurs="0" name="resultMessage" type="xs:string">
            <xs:annotation>
              <xs:documentation>
                                Brief acknowlegement message.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="createCarrierSuccessData" substitutionGroup="types:data" type="tfmi:CreateCarrierSuccessData" />
  <xs:complexType name="LookupDobEventsRequest">
    <xs:annotation>
      <xs:documentation>
                Lookup DAT Onboarding events received since a given date/time. Events include 
                changes to carrier profile, carrier W9 status, carrier insurance agents, 
                carrier attachments and contracts signed by a carrier.<br /><br />
                This request is only supported through the secure (HTTPS) endpoint. Requests 
                through the insecure endpoint will return an error.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="message:Request">
        <xs:sequence>
          <xs:element ref="tfmi:lookupDobEventsOperations" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobEventsRequest" substitutionGroup="message:request" type="tfmi:LookupDobEventsRequest" />
  <xs:complexType name="LookupDobEventsOperation">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Operation">
        <xs:sequence>
          <xs:element name="sinceDate" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>
                                The date and time to look for events.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="dobVersion" type="xs:int">
            <xs:annotation>
              <xs:documentation>
                                The version of response to receive.  The default is 1.
                                Set the version to the version of the XML schema that you built to.
                                The version of this schema is 2.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobEventsOperations" substitutionGroup="types:operations" type="tfmi:LookupDobEventsOperation" />
  <xs:complexType name="LookupDobEventsResponse">
    <xs:complexContent mixed="false">
      <xs:extension base="message:Response">
        <xs:sequence>
          <xs:element ref="tfmi:lookupDobEventsResults" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobEventsResponse" substitutionGroup="message:response" type="tfmi:LookupDobEventsResponse" />
  <xs:complexType name="LookupDobEventsResult">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Result">
        <xs:sequence>
          <xs:choice>
            <xs:element ref="tfmi:lookupDobEventsSuccessData" />
            <xs:element ref="types:serviceError" />
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobEventsResults" substitutionGroup="types:results" type="tfmi:LookupDobEventsResult" />
  <xs:complexType name="LookupDobEventsSuccessData">
    <xs:complexContent mixed="false">
      <xs:extension base="types:SuccessData">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="dobEvents" type="dobtypes:DobEvent">
            <xs:annotation>
              <xs:documentation>
                                The DAT-On-Boarding events that were received since the given date.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobEventsSuccessData" substitutionGroup="types:data" type="tfmi:LookupDobEventsSuccessData" />
  <xs:complexType name="LookupDobCarriersRequest">
    <xs:annotation>
      <xs:documentation>
                Lookup information on a DAT Onboarding carrier. All data available for the carrier 
                is returned.<br /><br />
                This request is only supported through the secure (HTTPS) endpoint. Requests 
                through the insecure endpoint will return an error.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="message:Request">
        <xs:sequence>
          <xs:element ref="tfmi:lookupDobCarriersOperations" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobCarriersRequest" substitutionGroup="message:request" type="tfmi:LookupDobCarriersRequest" />
  <xs:complexType name="LookupDobCarriersOperation">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Operation">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="dobVersion" type="xs:int">
            <xs:annotation>
              <xs:documentation>
                                The version of response to receive.  The default is 1.
                                Set the version to the version of the XML schema that you built to.
                                The version of this schema is 2.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="1" name="carrierId" type="types:String40">
            <xs:annotation>
              <xs:documentation>
                                The DAT-On-Boarding identifier for the carrier of interest.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobCarriersOperations" substitutionGroup="types:operations" type="tfmi:LookupDobCarriersOperation" />
  <xs:complexType name="LookupDobCarriersResponse">
    <xs:complexContent mixed="false">
      <xs:extension base="message:Response">
        <xs:sequence>
          <xs:element ref="tfmi:lookupDobCarriersResults" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobCarriersResponse" substitutionGroup="message:response" type="tfmi:LookupDobCarriersResponse" />
  <xs:complexType name="LookupDobCarriersResult">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Result">
        <xs:sequence>
          <xs:choice>
            <xs:element ref="tfmi:lookupDobCarriersSuccessData" />
            <xs:element ref="types:serviceError" />
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobCarriersResults" substitutionGroup="types:results" type="tfmi:LookupDobCarriersResult" />
  <xs:complexType name="LookupDobCarriersSuccessData">
    <xs:complexContent mixed="false">
      <xs:extension base="types:SuccessData">
        <xs:sequence>
          <xs:element name="dobCarrier" type="dobtypes:DobCarrier">
            <xs:annotation>
              <xs:documentation>
                                The DAT-On-Boarding data available for the specified carrier.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobCarriersSuccessData" substitutionGroup="types:data" type="tfmi:LookupDobCarriersSuccessData" />
  <xs:complexType name="LookupDobSignedCarriersRequest">
    <xs:annotation>
      <xs:documentation>
                Lookup all carriers that have signed contracts through DAT Onboarding with the 
                user. The intended usage is to synchronize the client's system with all relevant 
                data available in the DAT Onboarding system. All carriers which have ever signed 
                a contract with the customer and the contracts they have signed will be 
                returned.<br /><br />
                This request is only supported through the secure (HTTPS) endpoint. Requests 
                through the insecure endpoint will return an error.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="message:Request">
        <xs:sequence>
          <xs:element ref="tfmi:lookupDobSignedCarriersOperations" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobSignedCarriersRequest" substitutionGroup="message:request" type="tfmi:LookupDobSignedCarriersRequest" />
  <xs:complexType name="LookupDobSignedCarriersOperation">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Operation">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="dobVersion" type="xs:int">
            <xs:annotation>
              <xs:documentation>
                                The version of response to receive.  The default is 1.
                                Set the version to the version of the XML schema that you built to.
                                The version of this schema is 2.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobSignedCarriersOperations" substitutionGroup="types:operations" type="tfmi:LookupDobSignedCarriersOperation" />
  <xs:complexType name="LookupDobSignedCarriersResponse">
    <xs:complexContent mixed="false">
      <xs:extension base="message:Response">
        <xs:sequence>
          <xs:element ref="tfmi:lookupDobSignedCarriersResults" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobSignedCarriersResponse" substitutionGroup="message:response" type="tfmi:LookupDobSignedCarriersResponse" />
  <xs:complexType name="LookupDobSignedCarriersResult">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Result">
        <xs:sequence>
          <xs:choice>
            <xs:element ref="tfmi:lookupDobSignedCarriersSuccessData" />
            <xs:element ref="types:serviceError" />
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobSignedCarriersResults" substitutionGroup="types:results" type="tfmi:LookupDobSignedCarriersResult" />
  <xs:complexType name="LookupDobSignedCarriersSuccessData">
    <xs:complexContent mixed="false">
      <xs:extension base="types:SuccessData">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="signedCarriers" type="dobtypes:SignedCarrier">
            <xs:annotation>
              <xs:documentation>
                                The list of carriers and the contracts that they have signed with the user.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupDobSignedCarriersSuccessData" substitutionGroup="types:data" type="tfmi:LookupDobSignedCarriersSuccessData" />
  <xs:complexType name="Asset">
    <xs:annotation>
      <xs:documentation>
                This is the as-posted current state of a posting, i.e., the result of a
                PostAssetOperation plus all subsequent operations (deletes and updates) on the 
                asset since being booked by the Freight Matching Service.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="assetId" type="tfmi:AssetId">
        <xs:annotation>
          <xs:documentation>
                        The historically unique ID assigned to this asset by the Freight Matching 
                        Service.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="status" type="tfmi:FmeStatus">
        <xs:annotation>
          <xs:documentation>
                        The current status of this asset.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:element name="shipment" type="tfmi:Shipment">
          <xs:annotation>
            <xs:documentation>
                            Shipment-specific information.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="equipment" type="tfmi:Equipment">
          <xs:annotation>
            <xs:documentation>
                            Equipment-specific information.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element minOccurs="0" name="postersReferenceId" type="tfmi:ReferenceId">
        <xs:annotation>
          <xs:documentation>
                        Poster-supplied reference ID for this asset.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ltl" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        For shipments, if set true, then this shipment will not occupy a full 
                        truck. For equipment, if set true, then this equipment does not have enough
                        available capacity to carry a full shipment. The element "dimensions"
                        should be consulted for cargo/capacity size.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="2" name="comments" type="types:String70">
        <xs:annotation>
          <xs:documentation>
                        Optional poster-supplied comment(s) for this asset, displayed on search
                        match results and the Truck Stop Network.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="count" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        The number of assets represented by this description. Will always be at 
                        least 1.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="dimensions" type="tfmi:Dimensions">
        <xs:annotation>
          <xs:documentation>
                        For shipments, the dimensions of the cargo. For equipment, the dimensions 
                        of the available cargo capacity.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="stops" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        The number of stops this asset must make. Will always be at least 1.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="availability" type="tfmi:Availability">
        <xs:annotation>
          <xs:documentation>
                        For shipments, when the cargo is available for pickup by a truck. For 
                        equipment, when the truck is available to pickup a shipment.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Alarm">
    <xs:annotation>
      <xs:documentation>
                Information about an alarm. Includes its criteria and statistics.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="alarmId" type="tfmi:AlarmId">
        <xs:annotation>
          <xs:documentation>
                        The ID of this alarm.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="status" type="tfmi:FmeStatus" />
      <xs:element name="basisAssetId" type="tfmi:AssetId">
        <xs:annotation>
          <xs:documentation>
                        The asset this alarm is based on.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="basisAssetPostersReferenceId" type="tfmi:ReferenceId">
        <xs:annotation>
          <xs:documentation>
                        The optional poster-supplied reference ID of the basis asset. If the asset 
                        did not have a poster-supplied reference ID, or this alarm was not based 
                        on an asset, then this element is omitted.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="matchesRemaining" type="tfmi:AlarmMatchesCount">
        <xs:annotation>
          <xs:documentation>
                        Number of matches remaining that can be returned for this alarm.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="maxMatches" type="tfmi:AlarmMatchesCount">
        <xs:annotation>
          <xs:documentation>
                        Maximum number of matches that will be returned over the lifetime of this 
                        alarm.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="alarmCriteria" type="tfmi:AlarmSearchCriteria">
        <xs:annotation>
          <xs:documentation>
                        The alarm criteria supplied when this alarm was created (or last updated).
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="matchingAssetIds" type="tfmi:AssetId">
        <xs:annotation>
          <xs:documentation>
                        List of the IDs of the assets that have been matched by this alarm.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Search">
    <xs:annotation>
      <xs:documentation>
                This is the current state of a search, i.e., the result of a CreateSearchRequest
                plus all subsequent operations and state changes on the search since
                being booked by the Freight Matching Service.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="searchId" type="tfmi:SearchId">
        <xs:annotation>
          <xs:documentation>
                        The historically unique ID assigned to this search by the Freight Matching 
                        Service.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="status" type="tfmi:FmeStatus">
        <xs:annotation>
          <xs:documentation>
                        The current status of this search.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="specification" type="tfmi:SearchCriteria">
        <xs:annotation>
          <xs:documentation>
                        The search criteria used for this search.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AlarmSearchCriteria">
    <xs:annotation>
      <xs:documentation>
                Criteria for an alarm search.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="originRadius" type="tfmi:Mileage">
        <xs:annotation>
          <xs:documentation>
                        Radius around the asset's origin.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="destinationRadius" type="tfmi:Mileage">
        <xs:annotation>
          <xs:documentation>
                        Radius around the asset's destination.<br /><br />
                        If the asset has a non-Point destination (e.g., an Equipment with an
                        Area/Empty destination), then this field should be omitted; it will be
                        ignored anyway.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ageLimitMinutes" type="tfmi:AssetAgeLimitMinutes">
        <xs:annotation>
          <xs:documentation>
                        If this is non-zero, then the Alarm will do an initial live 
                        lookback-search, for assets that are no older than the specified number 
                        of minutes.<br /><br />
                        If set to zero or omitted, then no initial lookback-search will be
                        performed.<br /><br />
                        NOTE: if an alarm is being created as part of a PostAsset, then this field
                        has no effect: an initial lookback search is not possible when posting an
                        asset.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="maxMatches" type="tfmi:AlarmMatchesCount">
        <xs:annotation>
          <xs:documentation>
                        Maximum number of matches to return over the lifetime of this alarm.<br /><br />
                        Default is 25 matches.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="lifetimeMinutes">
        <xs:annotation>
          <xs:documentation>
                        Lifetime of this alarm.<br /><br />
                        If omitted the lifetime will default to system policy.<br />
                        Actual lifetimes are subject to system policies, particularly for 
                        end-of-day termination.
                    </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:minInclusive value="1" />
            <xs:maxInclusive value="1440" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="referenceId" type="tfmi:ReferenceId">
        <xs:annotation>
          <xs:documentation>
                        Client-supplied reference ID. This ID will be included in all alarm
                        notifications generated by this alarm. If this alarm-specific referenceId
                        is left unspecified, the referenceId of the associated asset will be used
                        instead (if that referenceId is specified).<br /><br />
                        Note that Connexion will <i>not</i> enforce any uniqueness constraints on 
                        <code>referenceId</code> (unlike <code>postersReferenceId</code> on 
                        a posting).
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="notifyAllApplications" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        If set to true, match, cancellation, and termination notifications
                        produced by this alarm will be sent not only to the application creating 
                        the alarm, but also to any other applications into which the recipient (the
                        alarm owner or members of the owner's sharing group) is currently logged 
                        in.<br /><br />
                        This attribute should only be used in the rare scenarios in which sharing
                        alarm matches across multiple applications is desirable.<br /><br />
                        Defaults to false.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FmeStatus">
    <xs:annotation>
      <xs:documentation>
                Status information for a freight matching entity (asset or search).
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="userId" type="types:UserId">
        <xs:annotation>
          <xs:documentation>
                        The ID of the owner of this freight matching entity.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="startDate" type="types:UTC">
        <xs:annotation>
          <xs:documentation>
                        When this entity becomes active.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="endDate" type="types:UTC">
        <xs:annotation>
          <xs:documentation>
                        When this entity is no longer active.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="created" type="types:UserTimeStamp">
        <xs:annotation>
          <xs:documentation>
                        When this entity was created.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="updated" type="types:UserTimeStamp">
        <xs:annotation>
          <xs:documentation>
                        When this entity was last updated. Will initially be set to the same as 
                        'created'.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="lastModified" type="types:UserTimeStamp">
        <xs:annotation>
          <xs:documentation>
                        When this entity was last modified. I.e., the latter of 'created'
                        and 'updated'.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="expired" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        True if this asset has expired and is no longer visible in searches.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SearchArea">
    <xs:annotation>
      <xs:documentation>
                A search area is a collection of states/provinces and/or zones.<br /><br />
                If no search area is desired, then use "SearchOpen".
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="types:Area" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SearchRadius">
    <xs:sequence>
      <xs:element name="place" type="tfmi:Place">
        <xs:annotation>
          <xs:documentation>
                        Specifies the target location at the center of the search radius.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="radius" type="tfmi:Mileage">
        <xs:annotation>
          <xs:documentation>
                        Radius around the search location.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SearchOpen">
    <xs:annotation>
      <xs:documentation>
                This type designates an empty search geography, also known as an "open" search.
                These are common in truck searches.<br /><br />
                Note: The Freight Matching Service will enforce its own policies regarding the
                proper use of open searches.<br /><br />
                Therefore, a request document which is valid according to this schema may well
                result in an error from the Freight Matching Service if an open search is not
                allowed in a particular case.<br /><br /><b>An open origin coupled with an open destination is never allowed.</b></xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="types:Open" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="GeoCriteria">
    <xs:annotation>
      <xs:documentation>
                Specification of a geographic component of a search. A search may be based on 
                either (1) area, (2) radius, or (3) be open.
            </xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="area" type="tfmi:SearchArea">
        <xs:annotation>
          <xs:documentation>
                        Search for postings matching a set of states and/or zones.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="radius" type="tfmi:SearchRadius">
        <xs:annotation>
          <xs:documentation>
                        Search for postings within a radius of a specific point. The radius 
                        measurement defaults to road mileage, but that default can be overridden 
                        to specify air mileage if desired. Connexion will enforce TransCore 
                        limitations on maximum radii.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="open" type="tfmi:SearchOpen">
        <xs:annotation>
          <xs:documentation>
                        Search for any postings, regardless of geography. Note that either an 
                        origin or destination must be provided. Open -&gt; open searches are not 
                        supported.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="SearchCriteria">
    <xs:annotation>
      <xs:documentation>
                Criteria for a search
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="assetType" type="types:AssetType">
        <xs:annotation>
          <xs:documentation>
                        Type of asset to search for (shipment or equipment).
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="equipmentClasses" type="types:EquipmentClass">
        <xs:annotation>
          <xs:documentation>
                        Match on assets belonging to any equipment class in this list. Each 
                        equipment class represents a predefined set of equipment types. If you 
                        need finer-grained control of which types are returned, you can specify 
                        one or more EquipmentTypes using the 'equipmentTypes' element.<br /><br />
                        Note that the 'Conestoga' class introduced in late 2012 is only supported 
                        for API versions &gt;= 2 (see LoginRequest).
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="equipmentTypes" type="types:EquipmentType">
        <xs:annotation>
          <xs:documentation>
                        Match on assets belonging to any equipment type in this list. Search by 
                        equipment type(s) was introduced in late 2012, and is only supported for 
                        API versions &gt;= 2 (see LoginRequest).
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ageLimitMinutes" type="tfmi:AssetAgeLimitMinutes">
        <xs:annotation>
          <xs:documentation>
                        Search for assets that are no older than this number of minutes. The age of
                        an asset is the time since it was created, or last updated,
                        or last refreshed, whichever is latest.<br /><br />
                        Note: An age of zero will result in no matches; this is usually not
                        desireable.<br /><br />
                        If omitted, defaults to 60 minutes.<br /><br />
                        NOTE: if an alarm is being created as part of a PostAsset, then this field
                        has no effect: an initial lookback search is not possible when posting an
                        asset.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="origin" type="tfmi:GeoCriteria">
        <xs:annotation>
          <xs:documentation>
                        Search origin.<br /><br />
                        Note that the origin and destination cannot both be 'open'. I.e., they 
                        both cannot specify a GeoCriteria member that is of type 'SearchOpen'.
                        All such search requests will be rejected with an error.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="destination" type="tfmi:GeoCriteria">
        <xs:annotation>
          <xs:documentation>
                        Search destination.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="availability" type="tfmi:Availability">
        <xs:annotation>
          <xs:documentation>
                        Search for assets that become available during specified limits. Assets
                        available prior to "earliest" (i.e., assets whose 'latest' availability is
                        prior to this criteria's 'earliest') will be excluded. Assets that are not
                        available until after 'latest' (i.e., assets whose 'earliest' availability is
                        after this criteria's 'latest') will be excluded.<br /><br />
                        If this specification is omitted, then it will default to (now, 
                        now+24hrs).<br /><br />
                        Note 1: "latest" (if specified) must be on or after 'earliest' (This
                        cannot be enforced with schema, since schema doesn't have relational
                        operators). A violation will result in defaulting 'latest' to 24
                        hours after 'earliest'. If 'earliest' is not specified, then it will be
                        defaulted to 24 hours prior to 'latest'.<br /><br />
                        Note 2: 'earliest' (if specified) must be on or prior to 'latest' (this 
                        cannot enforce with schema, since schema doesn't have relational 
                        operators). A violation will cause defaulting 'earliest' to 24 hours prior 
                        to 'latest'. If 'latest' is not specified, then it will be defaulted to 24 
                        hours after 'earliest'.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="includeLtls" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        If set true, when searching for shipments, include shipments that are marked
                        by their poster as "LTL" (i.e., the shipment does not occupy all the 
                        capacity of a truck).<br /><br />
                        If set true, when searching for equipment, include those that are marked
                        by their poster as "LTL" (i.e., only part of the equipment's capacity is
                        available).<br /><br />
                        If omitted, defaults to true.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="includeFulls" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        If set true, when searching for loads, include shipments that are "full"
                        loads (i.e., non-LTLs).<br /><br />
                        If set true, when searching for trucks, include trucks that are
                        not marked as LTL (i.e., they are available to carry a full shipment).<br /><br />
                        If omitted, defaults to true.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="limits" type="tfmi:Dimensions">
        <xs:annotation>
          <xs:documentation>
                        When searching for shipments, specifies the available capacity of the
                        searcher's equipment (i.e., the maximum available cargo size).<br /><br />
                        When searching for equipment, specifies the minimum cargo capacity needed
                        to haul the searcher's shipment.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="excludeOpenDestinationEquipment" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        If set true, trucks posted with a destination of <code>Open</code> will be 
                        excluded. On shipment searches, this element is ignored.<br /><br />
                        Note: This option is currently only supported for the CountAssetsRequest. 
                        It is ignored on the normal CreateSearchRequest and on alarms.<br /><br />
                        If omitted, defaults to false.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Dimensions">
    <xs:annotation>
      <xs:documentation>
                General purpose description of physical limits.<br /><br />
                When used to describe a posted cargo/shipment, it defines the size of the cargo,
                including pallets, shipping material, etc.<br /><br />
                When used to describe a posted equipment, it defines the available cargo capacity. 
                I.e., the dimensions of a shipment must fit inside the dimensions of the 
                equipment's capacity.<br /><br />
                On searches, these are regarded as limits. Any shipments with a cargo dimension
                greater than that specified here will be excluded. Any equipment with an available
                cargo space dimension less than that specified here will be excluded.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="lengthFeet">
        <xs:annotation>
          <xs:documentation>
                        Length of the shipment or truck deck in feet. 
                    </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:minInclusive value="1" />
            <xs:maxInclusive value="199" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="weightPounds">
        <xs:annotation>
          <xs:documentation>
                        Weight of the shipment or truck carrying capacity in pounds. 
                    </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:minInclusive value="1" />
            <xs:maxInclusive value="199999" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="heightInches">
        <xs:annotation>
          <xs:documentation>
                        Height of the shipment or truck box in inches. 
                    </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:minInclusive value="1" />
            <xs:maxInclusive value="299" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="volumeCubicFeet">
        <xs:annotation>
          <xs:documentation>
                        Volume of the shipment or truck box in cubic feet. 
                    </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:minInclusive value="1" />
            <xs:maxInclusive value="9999" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Availability">
    <xs:annotation>
      <xs:documentation>
                General purpose description of an availability window. An availability is a window 
                of opportunity for an asset. For shipments, it defines the earliest and latest the
                shipment can be picked up. For equipment, it defines the earliest and latest the
                equipment is available to pick up a shipment.<br /><br />
                In the course of freight matching, some portion of the availability windows of a
                shipment and a matching equipment (and vice versa) must overlap in order for the
                match to succeed.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="earliest" type="types:UTC">
        <xs:annotation>
          <xs:documentation>
                        For shipments, the earliest the shipment can be picked up. For equipment, 
                        the earliest the equipment is available to pick up a shipment.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="latest" type="types:UTC">
        <xs:annotation>
          <xs:documentation>
                        For shipments, the latest the shipment can be picked up. For equipment, 
                        the latest the equipment is available to pick up a shipment.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="SortOrder">
    <xs:annotation>
      <xs:documentation>
                How search matches are to be sorted.
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="None">
        <xs:annotation>
          <xs:documentation>
                        Do not sort. Fastest, but no notion of "best".
                    </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Closest">
        <xs:annotation>
          <xs:documentation>
                        Sort by closest-first (Origin Deadhead distance). Distance is computed 
                        based on the mileage type specified in the search request, if possible. 
                        If not, then air miles are used.
                    </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Alpha">
        <xs:annotation>
          <xs:documentation>
                        Sort alphabetically:
                        <ol><li>Origin State</li><li>Origin City</li><li>Destination State</li><li>Destination City</li></ol></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Age">
        <xs:annotation>
          <xs:documentation>
                        Sort by age, youngest first. Age is based on the elapsed time since the
                        matching asset was created, updated, or refreshed (whichever is most 
                        recent).
                    </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AssetAgeLimitMinutes">
    <xs:annotation>
      <xs:documentation>
                The oldest assets that will be included in a search<br /><br />
                The maximum is 99 hours (5940 minutes).
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="5940" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="MatchingAsset">
    <xs:annotation>
      <xs:documentation>
                A matching asset resulting from a search.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="asset" type="tfmi:Asset">
        <xs:annotation>
          <xs:documentation>
                        The matching asset.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="tripMileage" type="tfmi:Mileage">
        <xs:annotation>
          <xs:documentation>
                        The total mileage (and how it was computed: road or air miles) of the 
                        one-way origin -&gt; destination trip.<br /><br />
                        Omitted if the origin and destination are not both points.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="originDeadhead" type="tfmi:Mileage">
        <xs:annotation>
          <xs:documentation>
                        Deadhead mileage from the origin of the searcher to the origin of the 
                        asset, i.e., from the current origin of a truck, to the origin of the
                        shipment.<br /><br />
                        If deadhead mileage cannot be computed (e.g., origin is not a point),
                        then this item is omitted.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="destinationDeadhead" type="tfmi:Mileage">
        <xs:annotation>
          <xs:documentation>
                        Deadhead mileage from the destination of the searcher to the destination of
                        the asset. i.e., from the current destination of a truck, to the destination
                        of the shipment.<br /><br />
                        If deadhead mileage cannot be computed (e.g., destination is not a point),
                        then this item is omitted.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="callback" type="tfmi:PostingCallback">
        <xs:annotation>
          <xs:documentation>
                        Callback information on the poster of the matching asset, so the searcher 
                        can contact the poster.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="creditScore" type="tfmi:CreditScoreInfo">
        <xs:annotation>
          <xs:documentation>
                        Credit score of the poster's company
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="thirdPartyInfo" type="tfmi:ThirdPartyInfo">
        <xs:annotation>
          <xs:documentation>
                        Third party information on the poster's company.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="dotIds" type="tfmi:DotIds">
        <xs:annotation>
          <xs:documentation>
                        DOT ID information (DOT #, Docket #s, etc.)
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Shipment">
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:BaseAsset">
        <xs:sequence>
          <xs:element name="equipmentType" type="types:EquipmentType">
            <xs:annotation>
              <xs:documentation>
                                The type of equipment that is needed to haul this shipment. 
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="origin" type="tfmi:Place">
            <xs:annotation>
              <xs:documentation>
                                The origination point of this shipment.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="destination" type="tfmi:Place">
            <xs:annotation>
              <xs:documentation>
                                The destination point of this shipment.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="truckStops" type="tfmi:TruckStops">
            <xs:annotation>
              <xs:documentation>
                                All the information needed to advertise this shipment on the Truck
                                Stop network.<br /><br />
                                If omitted, this shipment will not be advertised on the Truck Stop
                                Network.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="rate" type="tfmi:ShipmentRate">
            <xs:annotation>
              <xs:documentation>
                                Payment rate in USD (flat-rate or per-mile).
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Equipment">
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:BaseAsset">
        <xs:sequence>
          <xs:element name="equipmentType" type="types:EquipmentType">
            <xs:annotation>
              <xs:documentation>
                                The type of this equipment.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="origin" type="tfmi:Place">
            <xs:annotation>
              <xs:documentation>
                                The origination point of this equipment.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="destination" type="tfmi:EquipmentDestination">
            <xs:annotation>
              <xs:documentation>
                                The desired destination (point, area, or open).
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ShipmentUpdate">
    <xs:annotation>
      <xs:documentation>
                Shipment-specific items which can be updated.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:AssetUpdate">
        <xs:sequence>
          <xs:element minOccurs="0" name="truckStops" type="tfmi:TruckStops">
            <xs:annotation>
              <xs:documentation>
                                Truckstop locations can be altered, but the number of truckstops
                                cannot be decreased.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="rate" type="tfmi:ShipmentRate">
            <xs:annotation>
              <xs:documentation>
                                Payment rate in USD (flat-rate or per-mile).
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="postToExtendedNetwork" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Post this shipment to the Extended Network. Defaults to false.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EquipmentUpdate">
    <xs:annotation>
      <xs:documentation>
                There are currently no equipment-specific items
                which can be updated.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:AssetUpdate" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AssetUpdate">
    <xs:annotation>
      <xs:documentation>
                Note that the updateable fields are all optional. Any field not specified for
                update will retain its current value. Once a field is set, its value can be
                changed, but there is no way to unset the field.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="ltl" type="xs:boolean" />
      <xs:element minOccurs="0" maxOccurs="2" name="comments" type="types:String70">
        <xs:annotation>
          <xs:documentation>
                        The number of comments on a posting exposed to truckstops cannot be
                        decreased. Omitting comments from an update is allowable (the previous
                        comment(s) will be left unchanged), but including a single comment when 
                        the posting currently includes 2 will result in an error.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="count" type="tfmi:AssetCount" />
      <xs:element minOccurs="0" name="dimensions" type="tfmi:Dimensions" />
      <xs:element minOccurs="0" name="stops" type="types:NonNegativeInt" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BaseAsset">
    <xs:annotation>
      <xs:documentation>
                .NET tools generate better code if all choice selectors have a common base type.
            </xs:documentation>
    </xs:annotation>
  </xs:complexType>
  <xs:complexType name="EquipmentDestination">
    <xs:annotation>
      <xs:documentation>
                Equipment destinations can be either a place or an area.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="place" type="tfmi:Place">
          <xs:annotation>
            <xs:documentation>
                            A specific location.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="area" type="types:Area">
          <xs:annotation>
            <xs:documentation>
                            A geographic area, defined by a list of states or a region.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="open" type="types:Open">
          <xs:annotation>
            <xs:documentation>
                            Represents "anywhere".
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Place">
    <xs:annotation>
      <xs:documentation>
                Places can be specified in a variety of ways. These choices are roughly in order
                of decreasing ambiguity:
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="types:GeographicLocation">
        <xs:sequence>
          <xs:choice>
            <xs:element name="postalCode" type="tfmi:FmPostalCode">
              <xs:annotation>
                <xs:documentation>
                                    USPS or Canada Post postal code. The Freight Matching Service
                                    will use the city+state/province assigned to this postal code).
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="cityAndState" type="tfmi:CityAndState">
              <xs:annotation>
                <xs:documentation>
                                    City + state/province. The Freight Matching Service will
                                    match the city + state/province against its atlas of known 
                                    cities. If multiple cities of the same name exist in the 
                                    state/province, it will use the optional county to 
                                    disambiguate).
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="namedPostalCode" type="tfmi:NamedPostalCode">
              <xs:annotation>
                <xs:documentation>
                                    Postal code + city + state/province. The Freight Matching
                                    Service will internally use the latitude/longitude coordinates 
                                    of the city assigned to the postal code, but will list the 
                                    specified city + state/province on assets/searches. This 
                                    option is useful when the popularly named city is not what 
                                    USPS/CP assigns to that postal code code (e.g., 97222 is 
                                    officially Portland, Oregon, but residents of that ZIP refer 
                                    to their locale as Milwaukie).
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="coordinates" type="tfmi:LatLon">
              <xs:annotation>
                <xs:documentation>
                                    Latitude/longitude coordinates. The Freight Matching Service 
                                    will internally assign the closest known city to those 
                                    coordinates. Note that this could result in a city being 
                                    selected that is not in the same state/province as the 
                                    coordinates.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="namedCoordinates" type="tfmi:NamedLatLon">
              <xs:annotation>
                <xs:documentation>
                                    City + state/province + coordinates. DAT Connexion will use 
                                    the city, state/province, and coordinates to resolve to a 
                                    known place. If an exact match is not found, Connexion will 
                                    use variations in the city spelling and proximity to the 
                                    coordinates to resolve. If no resolution is found, Connexion 
                                    will accept the specified city spelling as-is, and use the 
                                    specified coordinates and state/province (although road 
                                    mileages based on this city will not be possible).
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="place" type="tfmi:Place" />
  <xs:complexType name="CityAndState">
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:PlaceBase">
        <xs:sequence>
          <xs:element name="city" type="types:CityName">
            <xs:annotation>
              <xs:documentation>
                                City name.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="stateProvince" type="types:StateProvince">
            <xs:annotation>
              <xs:documentation>
                                State or province.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="county" type="types:String30">
            <xs:annotation>
              <xs:documentation>
                                County. If present, the county is used to disambiguate between 
                                multiple similarly named cities in the state or province.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="cityAndState" type="tfmi:CityAndState" />
  <xs:complexType name="NamedPostalCode">
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:CityAndState">
        <xs:sequence>
          <xs:element name="postalCode" type="types:PostalCode" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="namedPostalCode" type="tfmi:NamedPostalCode" />
  <xs:complexType name="LatLon">
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:PlaceBase">
        <xs:sequence>
          <xs:element name="latitude" type="types:Latitude">
            <xs:annotation>
              <xs:documentation>
                                Latitude of the desired point.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="longitude" type="types:Longitude">
            <xs:annotation>
              <xs:documentation>
                                Longitude of the desired point.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="latLon" type="tfmi:LatLon" />
  <xs:complexType name="NamedLatLon">
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:LatLon">
        <xs:sequence>
          <xs:element name="city" type="types:CityName">
            <xs:annotation>
              <xs:documentation>
                                City name.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="stateProvince" type="types:StateProvince">
            <xs:annotation>
              <xs:documentation>
                                State or province.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="namedLatLon" type="tfmi:NamedLatLon" />
  <xs:complexType name="FmPostalCode">
    <xs:annotation>
      <xs:documentation>
                North American Postal Codes
                <ul><li>Canada: LNL NLN</li><li>Mexico: NNNNN</li><li>US ZIP: NNNNN(-NNNN)</li></ul></xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:PlaceBase">
        <xs:sequence>
          <xs:element name="country" type="types:CountryCode">
            <xs:annotation>
              <xs:documentation>
                                Country code.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="code">
            <xs:annotation>
              <xs:documentation>
                                ZIP or postal code. The hyphen in 9-digit US ZIP codes and the 
                                space in Canadian postal codes are optional.  
                            </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="5" />
                <xs:maxLength value="10" />
                <xs:pattern value="[A-Z][0-9][A-Z] [0-9][A-Z][0-9]" />
                <xs:pattern value="[A-Z][0-9][A-Z][0-9][A-Z][0-9]" />
                <xs:pattern value="[0-9]{5}" />
                <xs:pattern value="[0-9]{5}(-[0-9]{4})" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PlaceBase">
    <xs:annotation>
      <xs:documentation>
                .NET tools generate better code if all choice selectors have a common base type.
            </xs:documentation>
    </xs:annotation>
  </xs:complexType>
  <xs:element name="placeBase" type="tfmi:PlaceBase" />
  <xs:complexType name="ShipmentRate">
    <xs:annotation>
      <xs:documentation>
                Rate offered for a shipment (of course, still subject to negotiation). The 
                description of a rate must include both the base rate, and how that rate was
                established.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="baseRateDollars">
        <xs:annotation>
          <xs:documentation>
                        Rate in US Dollars (the rateBasedOn element defines whether 
                        this is a flat rate or per-mile).
                    </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:float">
            <xs:minInclusive value="0.0" />
            <xs:maxInclusive value="99999.99" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="rateBasedOn" type="tfmi:RateBasedOnType">
        <xs:annotation>
          <xs:documentation>
                        Per-mile or flat-rate.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="rateMiles">
        <xs:annotation>
          <xs:documentation>
                        If the rate is based on per-mile, or flat-rate for a trip,
                        this is the mileage used by the poster. It is supplied by
                        the poster, and may not agree with HHG mileage or that computed
                        by mileage vendors.
                    </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:minInclusive value="0" />
            <xs:maxInclusive value="9999" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="RateBasedOnType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Flat" />
      <xs:enumeration value="PerMile" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="TruckStops">
    <xs:annotation>
      <xs:documentation>
                Contains the information needed for a Shipment to
                be advertised on the
                Truck Stop Network.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice minOccurs="0">
        <xs:annotation>
          <xs:documentation>
                        The truck stops on which the shipment is to be advertised.<br /><br />
                        Not all requesters may be able to freely select from the choices, depending
                        on their subscriptions, configuration, and TransCore policy.<br /><br />
                        If omitted (the typical case), defaults to 'closest'.
                    </xs:documentation>
        </xs:annotation>
        <xs:element name="truckStopIds" type="tfmi:TruckStopIds" />
        <xs:element name="closest" type="tfmi:ClosestTruckStops">
          <xs:annotation>
            <xs:documentation>
                            Select the truckstops closest to the shipment origin. The number of 
                            selected locations is determined by the poster's subscription and contract.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="alternateClosest" type="tfmi:AlternateClosestTruckStops">
          <xs:annotation>
            <xs:documentation>
                            Select the truckstops closest to an alternate origin point. This 
                            option is typically used to display the shipment on truckstops in 
                            the desired lane.<br /><br />
                            The number of selected locations is determined by the poster's 
                            subscription and contract.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element minOccurs="0" maxOccurs="2" name="enhancements" type="tfmi:TruckStopVideoEnhancement">
        <xs:annotation>
          <xs:documentation>
                        Optional truck stop load monitor video enhancements for this shipment's 
                        advertisement.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="posterDisplayName" type="types:TSNDisplayName">
        <xs:annotation>
          <xs:documentation>
                        The short name of the poster's company, for display on the truck stop load
                        monitors.<br /><br />
                        If omitted, the short name of the poster's company will default to that
                        contained in the poster's company record.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TruckStopSelection">
    <xs:annotation>
      <xs:documentation>
                .NET tools generate better code if all choice selectors have a common base type.
            </xs:documentation>
    </xs:annotation>
  </xs:complexType>
  <xs:complexType name="TruckStopIds">
    <xs:annotation>
      <xs:documentation>
                Which truck stops to advertise at. The maximum number of stops allowed is based on
                the requester's subscriptions, configuration, and TransCore policy.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:TruckStopSelection">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" name="ids" type="tfmi:TruckStopId">
            <xs:annotation>
              <xs:documentation>
                                A list of TransCore/DAT identifiers for the truckstops the shipment 
                                should be displayed at.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ClosestTruckStops">
    <xs:annotation>
      <xs:documentation>
                Tells the Freight Matching Service to use the largest, closest available truck 
                stops to the posting's origin.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:TruckStopSelection">
        <xs:sequence />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AlternateClosestTruckStops">
    <xs:annotation>
      <xs:documentation>
                Tells the Freight Matching Service to use the largest, closest available truck 
                stops <i>not</i> to the posting's origin, but closest to an alternate city.
                This is used in particular trip lanes where advertising near the posting's origin 
                is not effective; instead, advertising needs to be done further away in the same 
                lane.
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:TruckStopSelection">
        <xs:sequence>
          <xs:element name="alternateOrigin" type="tfmi:Place">
            <xs:annotation>
              <xs:documentation>
                                Origin point.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="TruckStopId">
    <xs:annotation>
      <xs:documentation>
                ID of a truck stop at which to advertise a shipment.
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="9999" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TruckStopVideoEnhancement">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Flash" />
      <xs:enumeration value="Highlight" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="PostingCallback">
    <xs:annotation>
      <xs:documentation>
                Callback contact information supplied on lead takes.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="userId" type="types:UserId">
        <xs:annotation>
          <xs:documentation>
                        The ID of the user that posted this asset.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:annotation>
          <xs:documentation>
                        How to contact the poster.
                    </xs:documentation>
        </xs:annotation>
        <xs:element name="phone" type="tfmi:CallbackPhoneNumber" />
        <xs:element name="email" type="tfmi:CallbackEmailAddress" />
      </xs:choice>
      <xs:element name="name" type="types:PersonName">
        <xs:annotation>
          <xs:documentation>
                        The first and last name of the poster.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="companyName" type="types:String50">
        <xs:annotation>
          <xs:documentation>
                        The full 50-char name of the company to which the poster belongs.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="displayCompany" type="types:TSNDisplayName">
        <xs:annotation>
          <xs:documentation>
                        The short 8-char name of the company to which the poster belongs.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postersStateProvince" type="types:StateProvince">
        <xs:annotation>
          <xs:documentation>
                        The state/province in which the poster resides.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CallbackPhoneNumber">
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:CallbackContact">
        <xs:sequence>
          <xs:element name="phone" type="types:PhoneNumber" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="callbackPhoneNumber" substitutionGroup="tfmi:callbackContact" type="tfmi:CallbackPhoneNumber" />
  <xs:complexType name="CallbackEmailAddress">
    <xs:complexContent mixed="false">
      <xs:extension base="tfmi:CallbackContact">
        <xs:sequence>
          <xs:element name="email" type="types:EmailAddress" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="callbackEmailAddress" substitutionGroup="tfmi:callbackContact" type="tfmi:CallbackEmailAddress" />
  <xs:complexType name="CallbackContact">
    <xs:annotation>
      <xs:documentation>
                .NET tools generate better code if all choice selectors have a common base type.
            </xs:documentation>
    </xs:annotation>
  </xs:complexType>
  <xs:element name="callbackContact" type="tfmi:CallbackContact" />
  <xs:simpleType name="AssetCount">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="99" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AlarmMatchesCount">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="3000" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="CreditScoreInfo">
    <xs:sequence>
      <xs:element name="score" type="xs:int" />
      <xs:element name="daysToPay" type="xs:int" />
      <xs:element name="scoreTimeStamp" type="xs:dateTime" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="creditScoreInfo" type="tfmi:CreditScoreInfo" />
  <xs:complexType name="ThirdPartyInfo">
    <xs:annotation>
      <xs:documentation>
                If any element in this type is missing in the instance document, 'false' is 
                assumed.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="rmisGreenLight" type="xs:boolean" />
      <xs:element minOccurs="0" name="nmftaMember" type="xs:boolean" />
      <xs:element minOccurs="0" name="ooidaMember" type="xs:boolean" />
      <xs:element minOccurs="0" name="tiaP3Member" type="xs:boolean" />
      <xs:element minOccurs="0" name="assurable" type="xs:boolean" />
      <xs:element minOccurs="0" name="rivieraGreenLight" type="xs:boolean" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="thirdPartyInfo" type="tfmi:ThirdPartyInfo" />
  <xs:complexType name="Mileage">
    <xs:annotation>
      <xs:documentation>
                A Distance value, and a description of how the distance was derived.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="miles" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Mileage.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="method" type="types:MileageType">
        <xs:annotation>
          <xs:documentation>
                        Mileage calculation method.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DotHeader">
    <xs:sequence>
      <xs:element name="dotNumber" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        DOT Identification Number of the carrier. If no DOT number is available, 
                        0 will be returned.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="docketNumber" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Docket Number of the carrier (MC, MX, or FF). If no Docket number is
                        available, 0 will be returned.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="dbaName" type="types:String155">
        <xs:annotation>
          <xs:documentation>
                        Commonly used business name.<br /><br />
                        Not included if DOT registration is no longer active.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="legalName" type="types:String155">
        <xs:annotation>
          <xs:documentation>
                        Legally registered business name.<br /><br />
                        Not included if DOT registration is no longer active.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="address" type="types:Address">
        <xs:annotation>
          <xs:documentation>
                        Address registered with the DOT. Not included if DOT registration
                        is no longer active.<br /><br />
                        Note that there are instances of incorrect registration data, in
                        which the postal code is not of the proper format for the designated
                        country code.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="phone" type="types:PhoneNumber">
        <xs:annotation>
          <xs:documentation>
                        Phone number registered with the DOT.<br /><br />
                        Not included if DOT registration is no longer active.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="fax" type="types:PhoneNumber">
        <xs:annotation>
          <xs:documentation>
                        Fax number registered with the DOT.<br /><br />
                        Not included if DOT registration is no longer active.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="mailingAddress" type="types:Address">
        <xs:annotation>
          <xs:documentation>
                        Mailing address registered with the DOT.<br /><br />
                        Not included if DOT registration is no longer active.<br /><br />
                        Note that there are instances of incorrect registration data, in
                        which the postal code is not of the proper format for the designated
                        country code.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="mailingPhone" type="types:PhoneNumber">
        <xs:annotation>
          <xs:documentation>
                        Mailing phone number registered with the DOT.<br /><br />
                        Not included if DOT registration is no longer active.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="mailingFax" type="types:PhoneNumber">
        <xs:annotation>
          <xs:documentation>
                        Mailing fax number registered with the DOT.<br /><br />
                        Not included if DOT registration is no longer active.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DotAuthority">
    <xs:sequence>
      <xs:element name="commonAuthority" type="tfmi:AuthorityStatus">
        <xs:annotation>
          <xs:documentation>
                        Status of this carrier's common authority.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="contractAuthority" type="tfmi:AuthorityStatus">
        <xs:annotation>
          <xs:documentation>
                        Status of this carrier's contract authority.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="brokerAuthority" type="tfmi:AuthorityStatus">
        <xs:annotation>
          <xs:documentation>
                        Status of this carrier's broker authority.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="pendingCommonAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        True if this carrier has common authority
                        pending.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="pendingContractAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        True if this carrier has contract authority
                        pending.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="pendingBrokerAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        True if this carrier has broker authority
                        pending.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="revokedCommonAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        True if this carrier is on 30-day notice for revocation. This occurs 30 
                        days prior to the expiration of the carrier's BIPD and/or Cargo insurance 
                        and continues until proof of new coverage has been received by the FMCSA. 
                        It also occurs if the carrier's insurance is about to be canceled.<br /><br />
                        Note: the carrier is still legal and authorized during this period.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="revokedContractAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        True if this carrier is on 30-day notice for revocation. This occurs 30 
                        days prior to the expiration of the carrier's BIPD and/or Cargo insurance 
                        and continues until proof of new coverage has been received by the FMCSA. 
                        It also occurs if the carrier's insurance is about to be canceled.<br /><br />
                        Note: the carrier is still legal and authorized during this period.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="revokedBrokerAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        True if this carrier is on 30-day notice for revocation. This occurs 30 
                        days prior to the expiration of the carrier's BIPD and/or Cargo insurance 
                        and continues until proof of new coverage has been received by the FMCSA. 
                        It also occurs if the carrier's insurance is about to be canceled.<br /><br />
                        Note: the carrier is still legal and authorized during this period.<br /><br />
                        If this element is not included, no authority record was retrieved.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="freightFlag" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not the carrier is authorized for freight.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="passengerFlag" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not the carrier is authorized for passengers.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="hhgFlag" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not the carrier is authorized for household goods.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bipdRequired" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Numeric field indicating the amount of BIPD coverage this carrier is 
                        required to have.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cargoRequired" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether this carrier is required to file form BMC-34 showing
                        evidence of cargo coverage.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bondSuretyRequired" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether this carrier is required to file form BMC-84 or 85
                        showing evidence of a broker bond or equivalent trust fund.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bipdOnFile" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        The amount of BIPD coverage on file. Coverage details will be available
                        in the active/pending insurance section.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cargoOnFile" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether this carrier has filed form BMC-34 showing evidence of
                        cargo coverage.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bondOnFile" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether this carrier has also filed form BMC-84 or 85 showing
                        evidence of a broker bond or trust fund.<br /><br />
                        If true, coverage details will be in the active/pending insurance section.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InsuranceDescription">
    <xs:annotation>
      <xs:documentation>
                Describes an insurance policy carried by the
                carrier.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="bipdPolicy" type="types:String90">
        <xs:annotation>
          <xs:documentation>
                        The policy identified for this insurance coverage
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="effectiveDate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        Date this insurance policy became effective.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="canceledDate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        Date this insurance policy was canceled.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="formCode">
        <xs:annotation>
          <xs:documentation>
            <ul>
              <li>34 = Cargo</li>
              <li>91,91X = BIPD</li>
              <li>82 = BIPD (Surety Bond)</li>
              <li>83 = Cargo (Surety Bond)</li>
              <li>84 = Property Brokers Surety Bond</li>
              <li>85 = Property Brokers Trust Fund Agreement</li>
            </ul>
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="34" />
            <xs:enumeration value="82" />
            <xs:enumeration value="83" />
            <xs:enumeration value="84" />
            <xs:enumeration value="85" />
            <xs:enumeration value="91" />
            <xs:enumeration value="91X" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="coverageFrom" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Indicates the lower limit of the coverage. Usually zero except for Excess 
                        BIPD insurance.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="coverageTo" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Indicates the upper limit of the coverage.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="companyName" type="types:String50">
        <xs:annotation>
          <xs:documentation>
                        Insurance company name.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="contact" type="types:String90">
        <xs:annotation>
          <xs:documentation>
                        Insurance company contact name.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="address" type="types:Address">
        <xs:annotation>
          <xs:documentation>
                        Insurance company address.<br /><br />
                        Note that there are instances in which the registration data is incorrect 
                        and the postal code is not of the proper format for the designated country 
                        code.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="phone" type="types:PhoneNumber">
        <xs:annotation>
          <xs:documentation>
                        Insurance company phone number.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="fax" type="types:PhoneNumber">
        <xs:annotation>
          <xs:documentation>
                        Insurance company fax number.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FmcsaSafetyRating">
    <xs:annotation>
      <xs:documentation>
                Safety rating(s) from FMCSA
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:sequence minOccurs="0">
        <xs:element name="date" type="xs:dateTime">
          <xs:annotation>
            <xs:documentation>
                            Date of the most recent FMCSA safety rating.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="rating">
          <xs:annotation>
            <xs:documentation>
                            Most recent FMCSA Rating.<br /><br />
                            If this element is not included, no safety record was found.<br /><br />
                            Note: there are a very large number of carriers that do not have a 
                            safety rating. Lack of a safety rating should not be construed as a 
                            negative mark against the carrier.
                        </xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Satisfactory" />
              <xs:enumeration value="Conditional" />
              <xs:enumeration value="Unsatisfactory" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
      <xs:sequence minOccurs="0">
        <xs:element name="reviewType">
          <xs:annotation>
            <xs:documentation>
                            Indicates the type of review that resulted in the most recent rating.
                            For more detailed information regarding the reviews go to
                            http://safer.fmcsa.dot.gov/saferhelp.aspx#SafetyRating
                        </xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Compliance Review" />
              <xs:enumeration value="Safety Review" />
              <xs:enumeration value="CR and Security Contact Review" />
              <xs:enumeration value="ID/Operations" />
              <xs:enumeration value="ID Operations" />
              <xs:enumeration value="Non-Ratable" />
              <xs:enumeration value="Educational Contact" />
              <xs:enumeration value="Security Contact Review" />
              <xs:enumeration value="Non-Ratable" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="reviewDate" type="xs:dateTime">
          <xs:annotation>
            <xs:documentation>
                            The date on which the review occurred.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FmcsaCrashInspection">
    <xs:annotation>
      <xs:documentation>
                Crash and inspection history maintained by FMCSA
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="totalInspections" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Number of roadside inspections for this carrier conducted within the last 
                        24 months. Inspections are listed as total, driver, vehicle, and 
                        Hazmat.<br /><br />
                        There are five levels of inspections:
                        <ul><li>Level 1 - a complete inspection</li><li>Level 2 - a walk-around inspection</li><li>Level 3 - a driver-only inspection</li><li>Level 4 - an inspection for a special study</li><li>Level 5 - a vehicle-only inspection</li></ul>
                        Note that total inspections may not be the sum of the totals described
                        below. The number of inspections is likely to vary with the size of the
                        fleet.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="vehicleInspections" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Total number of vehicle inspections for this carrier in the last 24 
                        months. Levels 1+2+5.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="driverInspections" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Total number of driver inspections for this carrier in the last 24 months. 
                        Levels 1+2+3.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="hazmatInspections" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Total number of HazMar inspections for this
                        carrier in the last
                        24
                        months. Levels 1+2+3+4+5 when HazMat is
                        present.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="vehicleOOS" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Total number of vehicle inspections for this carrier that resulted in an 
                        Out of Service condition.<br /><br />
                        Note: Current national average for vehicle inspections resulting in an out 
                        of service condition is 23.14%.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="driverOOS" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Total number of driver inspections for this carrier that resulted in an 
                        Out of Service condition.<br /><br />
                        Note: Current national average for vehicle inspections resulting in an out 
                        of service condition is 6.8%.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="hazmatOOS" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Total number of hazmat inspections for this carrier that resulted in an
                        Out of Service condition.<br /><br />
                        Note: Current national average for vehicle inspections resulting in an out 
                        of service condition is 5.39%.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="crashesTotal" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Total number of crashes for this carrier in the last 24 months.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="crashesFatal" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Number of crashes for this carrier in the last 24 months that resulted in
                        a fatality.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="crashesInjury" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Number of crashes for this carrier in the last 24 months that resulted in
                        an injury.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="crashesTow" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Number of crashes for this carrier in the last 24 months that resulted in
                        the vehicle being towed.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FmcsaSafeStat">
    <xs:annotation>
      <xs:documentation>
                Safestat ratings maintained by FMCSA.<br /><br />
                A number of these ratings are quite unusual, so they will only be included when the
                score is non-zero.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="seaAccident" type="xs:float">
        <xs:annotation>
          <xs:documentation>
                        Most recent SEA_Accident score for this carrier provided by SafeStat. 
                        Ranges from 0 (best) to 100 (worst). Expect 2 decimal places. A SEA
                        with a value from 75 to 100 is defined as deficient.<br /><br />
                        This element will be omitted if the score is 0.<br /><br />
                        Note: The FMCSA has temporarily removed the Accident_SEA scores from public
                        viewing. This field has been included to prevent the necessity of changing 
                        the schema at a future date. For more information on why the FMCSA has 
                        temporarily removed this data go to
                        http://ai.fmcsa.dot.gov/SafeStat/Removal_SafeStat_Explain.asp.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="seaDriver" type="xs:float">
        <xs:annotation>
          <xs:documentation>
                        Most recent SEA_Driver score for this carrier provided by SafeStat. Ranges
                        from 0 (best) to 100 (worst). Expect 2 decimal places. A SEA with a value from
                        75 to 100 is defined as deficient.<br /><br />
                        This element will be omitted if the score is 0.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="seaVehicle" type="xs:float">
        <xs:annotation>
          <xs:documentation>
                        Most recent SEA_Vehicle score for this carrier provided by SafeStat.
                        Ranges from 0 (best) to 100 (worst). Expect 2 decimal places. A SEA
                        with a value from 75 to 100 is defined as deficient.<br /><br />
                        This element will be omitted if the score is 0.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="seaManagement" type="xs:float">
        <xs:annotation>
          <xs:documentation>
                        Most current SEA_Management score for this carrier provided by SafeStat.
                        SEA values range from 0 (best) to 100 (worst). Expect 2 decimal
                        places. A SEA with a value from 75 to 100 is defined as deficient.<br /><br />
                        This element will be omitted if the score is 0.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="safestatScore" type="xs:float">
        <xs:annotation>
          <xs:documentation>
                        Most current SafeStat score calculated for this carrier.The primary 
                        purpose of the SafeStat score is to identify and prioritize carriers for
                        FMCSA safety improvement programs.<br /><br />
                        For this purpose, SafeStat does not give SafeStat scores to all carriers.
                        To obtain a SafeStat score, a carrier must be deficient in at least two
                        SEAs. A SEA with a value from 75 to 100 is defined as deficient.<br /><br />
                        This range approximates the worst 25% of the carriers assessed within a
                        particular SEA. Therefore, SafeStat requires a "critical mass" of poor
                        performance data before a carrier is scored. Carriers that meet the
                        criterion of two deficient SEAs are given a SafeStat score that is equal
                        to the sum of the deficient SEA values for the Vehicle and Safety 
                        Management SEAs, plus 2 times the deficient Accident SEA Value plus 1.5 
                        times the deficient Driver SEA value.<br /><br />
                        SEA values that are less than 75 are not used by SafeStat in calculating 
                        the SafeStat score.<br /><br />
                        Note: The FMCSA has temporarily removed the SafeState score from public
                        viewing. This field has been included to prevent the necessity of changing
                        the schema at a future date. For more information on why the FMCSA has
                        temporarily removed this data go to
                        http://ai.fmcsa.dot.gov/SafeStat/Removal_SafeStat_Explain.asp.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="safestatCategory">
        <xs:annotation>
          <xs:documentation>
                        Most recent SafeStat Category assigned to this carrier. SafeStat categories
                        are assigned to carriers with safety deficiencies. SafeStat assigns each
                        scored carrier into Category A, B, or C, as defined by the SafeStat score
                        ranges below:
                        <ul><li>A = 350 to 550</li><li>B = 225 to 350</li><li>C = 150 to 225</li></ul>
                        Note: The FMCSA has temporarily removed the SafeState category from public
                        viewing. This field has been included to prevent the necessity of changing 
                        the schema at a future date. For more information on why the FMCSA has 
                        temporarily removed this data go to
                        http://ai.fmcsa.dot.gov/SafeStat/Removal_SafeStat_Explain.asp.
                    </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="A" />
            <xs:enumeration value="B" />
            <xs:enumeration value="C" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="postedDate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The date of the most recent SafeStat update. All numbers provided in this
                        section are accurate as of this date.<br /><br />
                        SafeStat recalculates SEA Values every month.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DotProfile">
    <xs:sequence>
      <xs:element minOccurs="0" name="duns" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        DUNS Number
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="powerUnits" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        The number of power units operated by the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="companyDrivers" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        The number of drivers employed by the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="contractDrivers" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        The number of drivers contracted by the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="commodities" type="tfmi:CommodityType">
        <xs:annotation>
          <xs:documentation>
                        Commodities handled by the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="specialCommodities" type="xs:string">
        <xs:annotation>
          <xs:documentation>
                        Free form text containing a description of commodities not covered in the 
                        standard DOT commodity list.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="operatingStates" type="types:StateProvince">
        <xs:annotation>
          <xs:documentation>
                        States in which the carrier operates.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="InsuranceType">
    <xs:annotation>
      <xs:documentation>
                Enumerated list of insurance types.
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="BIPD" />
      <xs:enumeration value="Cargo" />
      <xs:enumeration value="Bond" />
      <xs:enumeration value="Trust" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="InsuranceClass">
    <xs:annotation>
      <xs:documentation>
                Enumerated list of insurance classes.
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Primary" />
      <xs:enumeration value="Excess" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AuthorityStatus">
    <xs:annotation>
      <xs:documentation>
                Enumerated list of authority statuses (for Common Authority, Contract Authority, 
                etc.
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Active" />
      <xs:enumeration value="Inactive" />
      <xs:enumeration value="None" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CommodityType">
    <xs:annotation>
      <xs:documentation>
                Enumerated list of recognized commodities.
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="General Freight" />
      <xs:enumeration value="Fresh Produce" />
      <xs:enumeration value="Meat" />
      <xs:enumeration value="Agricultural/Farm Supplies" />
      <xs:enumeration value="Garbage/Refuse" />
      <xs:enumeration value="Metals: sheets, coils, rolls" />
      <xs:enumeration value="Beverages" />
      <xs:enumeration value="Grain, Feed Hay" />
      <xs:enumeration value="Mobile Homes" />
      <xs:enumeration value="Building Materials" />
      <xs:enumeration value="Household Goods" />
      <xs:enumeration value="Motor Vehicles" />
      <xs:enumeration value="Chemicals" />
      <xs:enumeration value="Liquids/Gases" />
      <xs:enumeration value="Oilfield Equipment" />
      <xs:enumeration value="Coal/Coke" />
      <xs:enumeration value="Livestock" />
      <xs:enumeration value="Paper Products" />
      <xs:enumeration value="Construction" />
      <xs:enumeration value="Logs, Poles, Beams, Lumber" />
      <xs:enumeration value="Refrigerated Food" />
      <xs:enumeration value="Dry Bulk" />
      <xs:enumeration value="Machinery, Large Objects" />
      <xs:enumeration value="Utilities" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ReferenceId">
    <xs:annotation>
      <xs:documentation>
                Private ID assigned by posters to their assets, for their own tracking 
                purposes.<br /><br />
                The Freight Matching Service provides access to posted assets by their poster
                reference ID as a convenience to requesters who would rather deal with their own 
                IDs than with the Freight Matching Service's IDs.
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="types:String8" />
  </xs:simpleType>
  <xs:simpleType name="AssetId">
    <xs:annotation>
      <xs:documentation>
                The Freight Matching Service assigns historically unique IDs to all assets. This 
                is the primary key by which assets are accessed (although the poster-supplied
                reference ID can also be used, for convenience; see "ReferenceId").
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="tfmi:FmcsId" />
  </xs:simpleType>
  <xs:simpleType name="AlarmId">
    <xs:annotation>
      <xs:documentation>
                The Freight Matching Service assigns historically unique IDs to all alarms. This 
                is the primary key by which alarm are accessed (although the poster-supplied
                reference ID can also be used, for convenience; see "ReferenceId").
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="tfmi:FmcsId" />
  </xs:simpleType>
  <xs:simpleType name="SearchId">
    <xs:annotation>
      <xs:documentation>
                The Freight Matching Service assigns historically unique IDs to all searches. This 
                is the primary key by which searches are accessed.
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="tfmi:FmcsId" />
  </xs:simpleType>
  <xs:simpleType name="FmcsId">
    <xs:restriction base="types:String8">
      <xs:pattern value="[D,L,C,K,M,S,T][A,B,E,F,L,S][A-Z,a-z,0-9]{6}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="DotIds">
    <xs:annotation>
      <xs:documentation>
                All of the DOT IDs that may be associated with a freight participant. There should
                be at most only one of any type of MC number associated with a DOT number.<br /><br />
                MC numbers should always be associated with a single DOT number (although TransCore
                does not always know the DOT number).<br /><br />
                Freight participants can have multiple DOT numbers (due to mergers), and therefore,
                multiple MC numbers of the same type, but each MC number should be registered to a
                different DOT number.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="dotNumber" type="types:DOTNumber" />
      <xs:element minOccurs="0" name="brokerMcNumber" type="types:MCNumber" />
      <xs:element minOccurs="0" name="carrierMcNumber" type="types:MCNumber" />
      <xs:element minOccurs="0" name="freightForwarderMcNumber" type="types:MCNumber" />
      <xs:element minOccurs="0" name="mexicoMcNumber" type="types:MCNumber" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LookupCapabilitiesRequest">
    <xs:annotation>
      <xs:documentation>
                Look up presence or absence of specific capabilities. Each user has the ability to
                perform certain operations within TransCore's system, depending on their active
                subscriptions. For example, only users with a premium CarrierWatch subscription
                are able to lookup insurance certificate information. A TMS system may want to
                alter its UI depending on the capabilities of the current user, so as to avoid
                presenting the user with functionality that will fail (e.g., removing a 'Search'
                tab for a user with a post-only subscription).
            </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="message:Request">
        <xs:sequence>
          <xs:element ref="tfmi:lookupCapabilitiesOperation" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCapabilitiesRequest" substitutionGroup="message:request" type="tfmi:LookupCapabilitiesRequest" />
  <xs:complexType name="LookupCapabilitiesOperation">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Operation">
        <xs:sequence>
          <xs:element maxOccurs="7" name="capability" type="tfmi:CapabilityType">
            <xs:annotation>
              <xs:documentation>
                                Capability or capabilities of interest. A boolean result will be
                                returned for each capability requested, in the same order as
                                specified in the request.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCapabilitiesOperation" substitutionGroup="types:operations" type="tfmi:LookupCapabilitiesOperation" />
  <xs:complexType name="LookupCapabilitiesResponse">
    <xs:complexContent mixed="false">
      <xs:extension base="message:Response">
        <xs:sequence>
          <xs:element minOccurs="0" ref="tfmi:lookupCapabilitiesResult" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCapabilitiesResponse" substitutionGroup="message:response" type="tfmi:LookupCapabilitiesResponse" />
  <xs:complexType name="LookupCapabilitiesResult">
    <xs:complexContent mixed="false">
      <xs:extension base="types:Result">
        <xs:sequence>
          <xs:choice>
            <xs:element ref="tfmi:lookupCapabilitiesSuccessData" />
            <xs:element ref="types:serviceError" />
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCapabilitiesResult" substitutionGroup="types:results" type="tfmi:LookupCapabilitiesResult" />
  <xs:complexType name="LookupCapabilitiesSuccessData">
    <xs:complexContent mixed="false">
      <xs:extension base="types:SuccessData">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" name="capabilityPresent" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                                Result of the capability query, in the same order as specified in 
                                the request.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lookupCapabilitiesSuccessData" substitutionGroup="types:data" type="tfmi:LookupCapabilitiesSuccessData" />
  <xs:simpleType name="CapabilityType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Manage Postings" />
      <xs:enumeration value="Search" />
      <xs:enumeration value="Alarm Match" />
      <xs:enumeration value="Lookup Spot Rates" />
      <xs:enumeration value="Lookup Contract Rates" />
      <xs:enumeration value="Lookup Basic Carrier Info" />
      <xs:enumeration value="Lookup Premium Carrier Info" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
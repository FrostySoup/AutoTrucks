  </xs:complexType>
  <xs:complexType name="FmcsaSafetyRating">
    <xs:annotation>
      <xs:documentation>
                FMCSA Safety Rating of the carrier queried.
                Provided when Safety is selected for download.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdateOfSafety" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        Date of the most recent FMCSA safety rating.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="rating">
        <xs:annotation>
          <xs:documentation>
                        Most recent FMCSA Rating.<br /><br />
                        If this element is not included, no safety record was found.<br /><br />
                        Note: there are a very large number of carriers that do not have a safety 
                        rating. Lack of a safety rating should not be construed as a negative 
                        mark against the carrier.
                    </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="types:String15">
            <xs:enumeration value="Satisfactory" />
            <xs:enumeration value="Conditional" />
            <xs:enumeration value="Unsatisfactory" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ratingDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>
                        The date the rating was assigned.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="reviewType" type="types:String40">
        <xs:annotation>
          <xs:documentation>
                        Indicates the type of review that resulted in the most recent rating. 
                        For more detailed information regarding the reviews go to 
                        http://safer.fmcsa.dot.gov/saferhelp.aspx#SafetyRating
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="reviewDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>
                        The date on which the review occurred.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FmcsaInspections">
    <xs:annotation>
      <xs:documentation>
                FMCSA Inspection data on the carrier.
                Provided when Safety is selected for download.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdateOfInspections" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        Date of the most recent FMCSA inspection data.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="totalInspections" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Total number of inspections for this carrier in the last 24 months. 
                        May be less than the sum of vehicle, driver, and hazmat inspections. 
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="vehicleInspections" type="cwtypes:FmcsaInspection">
        <xs:annotation>
          <xs:documentation>
                        Vehicle inspections for this carrier in the last 24 months. Levels 1+2+5.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="driverInspections" type="cwtypes:FmcsaInspection">
        <xs:annotation>
          <xs:documentation>
                        Driver inspections for this carrier in the last 24 months. Levels 1+2+3.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="hazmatInspections" type="cwtypes:FmcsaInspection">
        <xs:annotation>
          <xs:documentation>
                        Hazmat inspections for this carrier in the last 24 months. 
                        Levels 1+2+3+4+5 when HazMat is present.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FmcsaCrashes">
    <xs:annotation>
      <xs:documentation>
                FMCSA Crash data on the carrier.
                Provided when Safety is selected for download.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdateOfCrashes" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        Date of the most recent FMCSA inspection data.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="crashesFatal" type="types:NonNegativeInt">
            <xs:annotation>
              <xs:documentation>
                                Number of crashes for this carrier in the last 24 months that resulted in 
                                a fatality. May not be provided after CSA2010.  May only provide 
                                <code>crashesFatalOrInjury</code>.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="crashesInjury" type="types:NonNegativeInt">
            <xs:annotation>
              <xs:documentation>
                                Number of crashes for this carrier in the last 24 months that resulted in 
                                an injury.  May not be provided after CSA2010. May only provide 
                                <code>crashesFatalOrInjury</code>.
                            </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" name="crashesFatalOrInjury" type="types:NonNegativeInt">
          <xs:annotation>
            <xs:documentation>
                            Number of crashes for this carrier in the last 24 months that resulted in 
                            an injury or a fatality.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element minOccurs="0" maxOccurs="1" name="crashesTow" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Number of crashes for this carrier in the last 24 months that resulted in 
                        the vehicle being towed.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="crashesHazmat" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Number of crashes for this carrier in the last 24 months that involved 
                        hazardous materials.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FmcsaSafeStat">
    <xs:annotation>
      <xs:documentation>
                FMCSA SafeStat on the carrier queried.<br /><br />
                A number of these ratings are quite unusual, so they will only be included 
                when the score is non-zero. 
                Provided when Safety is selected for download.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdateOfSafeStat" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The date of the most recent SafeStat update. All numbers provided in 
                        this section are accurate as of this date.<br /><br />
                        Safestat recalculates SEA Values every month. 
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="seaDriver" type="xs:float">
        <xs:annotation>
          <xs:documentation>
                        Most recent SEA_Driver score for this carrier provided by SafeStat. 
                        Ranges from 0 (best) to 100 (worst). Expect 2 decimal places. A SEA 
                        with a value from 75 to 100 is defined as deficient.<br /><br />
                        This element will be omitted if the score is 0.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="seaVehicle" type="xs:float">
        <xs:annotation>
          <xs:documentation>
                        Most recent SEA_Vehicle score for this carrier provided by SafeStat. 
                        Ranges from 0 (best) to 100 (worst). Expect 2 decimal places. A SEA 
                        with a value from 75 to 100 is defined as deficient.<br /><br />
                        This element will be omitted if the score is 0.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="seaManagement" type="xs:float">
        <xs:annotation>
          <xs:documentation>
                        Most current SEA_Management score for this carrier provided by SafeStat. 
                        SEA values range from 0 (best) to 100 (worst). Expect 2 decimal places. 
                        A SEA with a value from 75 to 100 is defined as deficient.<br /><br /> 
                        This element will be omitted if the score is 0.<br /></xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

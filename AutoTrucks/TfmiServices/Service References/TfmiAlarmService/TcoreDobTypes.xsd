<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:dobtypes="http://www.tcore.com/TcoreDobTypes.xsd" xmlns:types="http://www.tcore.com/TcoreTypes.xsd" xmlns:cwtypes="http://www.tcore.com/TcoreCarrierWatchTypes.xsd" elementFormDefault="qualified" targetNamespace="http://www.tcore.com/TcoreDobTypes.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="TcoreTypes.xsd" namespace="http://www.tcore.com/TcoreTypes.xsd" />
  <xs:import schemaLocation="TcoreCarrierWatchTypes.xsd" namespace="http://www.tcore.com/TcoreCarrierWatchTypes.xsd" />
  <xs:annotation>
    <xs:documentation>
      <h2>TransCore DAT-On-Boarding XML Schema</h2>
      <br />
            This schema defines the types used by DAT On-Boarding.  
            <br />
            Published:  April, 2012<br />
            Version  :  1.0.0<br /><br /><i>Copyright (C) 2011, TransCore LP, All Rights Reserved.</i><br /><br /><br /></xs:documentation>
  </xs:annotation>
  <xs:complexType name="DobEvent">
    <xs:annotation>
      <xs:documentation>
                The events that occurred for a Carrier.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="carrierId" type="types:String40">
        <xs:annotation>
          <xs:documentation>
                        The DAT-On-Boarding identifier for the carrier that signed contracts.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="eventTypes" type="dobtypes:EventTypes">
        <xs:annotation>
          <xs:documentation>
                        The types of events that were received concerning this carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="signedContracts" type="dobtypes:SignedContract">
        <xs:annotation>
          <xs:documentation>
                        The contracts that this carrier signed since the date provided.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="EventTypes">
    <xs:annotation>
      <xs:documentation>
                The possible types of events that may be returned.
                    Contract Signed, One or more contracts were signed by the carrier.
                    Profile Update, a change was made to the profile data.
                    Insurance Agent Update, a change was made to insurance agent information.
                    Tax Info Update, a change was made to the W9 or W8 tax information (whichever applies).
            </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Contract Signed" />
      <xs:enumeration value="Profile Update" />
      <xs:enumeration value="Insurance Agent Update" />
      <xs:enumeration value="Tax Info Update" />
      <xs:enumeration value="Document Update" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="SignedCarrier">
    <xs:annotation>
      <xs:documentation>
                The carrier identifier and contracts that have been signed by them.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="carrierId" type="types:String40">
        <xs:annotation>
          <xs:documentation>
                        The DAT-On-Boarding identifier for the carrier that signed contracts.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="signedContracts" type="dobtypes:SignedContract">
        <xs:annotation>
          <xs:documentation>
                        The contracts that this carrier signed.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SignedContract">
    <xs:annotation>
      <xs:documentation>
                A contract and the person who electronically signed it.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="contract" type="dobtypes:Contract">
        <xs:annotation>
          <xs:documentation>
                        The contract that was signed.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="signatory" type="dobtypes:Signatory">
        <xs:annotation>
          <xs:documentation>
                        The signature information for the contract.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="signed" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The date the contract was signed.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="url" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation>
                        The URL from which this contract may be downloaded.  
                        Provided only when dobVersion&gt;1 and for contracts signed through AssureSign.
                        Max 256 characters.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Contract">
    <xs:annotation>
      <xs:documentation>
                The attributes that identify a COB Customer's contract.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="name" type="types:String100">
        <xs:annotation>
          <xs:documentation>
                        The Name of the contract.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="description" type="types:String100">
        <xs:annotation>
          <xs:documentation>
                        The description of the contract.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="version" type="types:PositiveInt">
        <xs:annotation>
          <xs:documentation>
                        The version of the contract.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="documentStatus" type="dobtypes:DocumentStatusTypes">
        <xs:annotation>
          <xs:documentation>
                        The status of the contract (Active or Inactive);
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Signatory">
    <xs:annotation>
      <xs:documentation>
                The information about the person that signed the contract.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="name" type="types:String100">
        <xs:annotation>
          <xs:documentation>
                        The Name of person signing a contract.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="title" type="types:String100">
        <xs:annotation>
          <xs:documentation>
                        The title of the person signing a contract.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DobCarrier">
    <xs:annotation>
      <xs:documentation>
                All attributes for a carrier.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The last date and time anything for this carrier was updated.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="carrierInfo" type="dobtypes:CarrierInfo">
        <xs:annotation>
          <xs:documentation>
                        The carrier identifiers, names, addresses and phone numbers.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="paymentInfo" type="dobtypes:PaymentInfo">
        <xs:annotation>
          <xs:documentation>
                        The payment information for this carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="contactInfo" type="dobtypes:ContactInfo">
        <xs:annotation>
          <xs:documentation>
                        The payment information for this carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="services" type="dobtypes:Services">
        <xs:annotation>
          <xs:documentation>
                        The services that this carrier provides.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="equipment" type="dobtypes:Equipment">
        <xs:annotation>
          <xs:documentation>
                        The equipment that this carrier has.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="geographicCoverage" type="dobtypes:GeographicCoverage">
        <xs:annotation>
          <xs:documentation>
                        The geographic areas that this carrier services.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="taxInfo" type="dobtypes:TaxInfo">
        <xs:annotation>
          <xs:documentation>
                        The tax information for this carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="insuranceAgentInfo" type="dobtypes:InsuranceAgentInfo">
        <xs:annotation>
          <xs:documentation>
                        The information about the carrier's insurance agents.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="documents" type="dobtypes:Document">
        <xs:annotation>
          <xs:documentation>
                        The documents that this carrier provides.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TaxInfo">
    <xs:annotation>
      <xs:documentation>
                A carrier's tax information, either W9 or W8 tax info.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The last date and time the tax info was updated.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:element name="w9Info" type="dobtypes:W9Info">
          <xs:annotation>
            <xs:documentation>
                            The W9 tax form information provided by the carrier.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="w8Info" type="dobtypes:W8Info">
          <xs:annotation>
            <xs:documentation>
                            The W8 tax form information provided by the carrier.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="W9Info">
    <xs:annotation>
      <xs:documentation>
                A carrier's W9 tax information.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="tin" type="types:FederalTaxIdentifier">
        <xs:annotation>
          <xs:documentation>
                        The federal tax id number.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="type" type="dobtypes:FederalTaxIdTypes">
        <xs:annotation>
          <xs:documentation>
                        The type of tax id provided, SSN or EIN.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="status" type="dobtypes:FederalTaxIdStatus">
        <xs:annotation>
          <xs:documentation>
                        The status of the federal tax id: Pending, Valid, or NotValid.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="name" type="types:String155">
        <xs:annotation>
          <xs:documentation>
                        The carrier name as shown on their income tax return.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="businessName" type="types:String155">
        <xs:annotation>
          <xs:documentation>
                        The carrier business name/disregard entity name, if different from above.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="street" type="types:String200">
        <xs:annotation>
          <xs:documentation>
                        The street address.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="city" type="types:String50">
        <xs:annotation>
          <xs:documentation>
                        The city address.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="stateProvince" type="types:StateProvince">
        <xs:annotation>
          <xs:documentation>
                        The State or Province.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postalCode" type="types:PostalCodePattern">
        <xs:annotation>
          <xs:documentation>
                        The Postal code.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="federalTaxClassification" type="dobtypes:FederalTaxClassification">
        <xs:annotation>
          <xs:documentation>
                        The W9 Fedearal Tax Classification.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="W8Info">
    <xs:annotation>
      <xs:documentation>
                A carrier's W8 tax information.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="name" type="types:String155">
        <xs:annotation>
          <xs:documentation>
                        The name of individual or organization that is the beneficial owner.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="typeOfBeneficialOwner" type="dobtypes:FederalTaxBeneficialOwnerTypes">
        <xs:annotation>
          <xs:documentation>
                        The type of beneficial owner.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="permenantAddress" type="cwtypes:Location">
        <xs:annotation>
          <xs:documentation>
                        The permenant address.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="mailingAddress" type="cwtypes:Location">
        <xs:annotation>
          <xs:documentation>
                        The mailing address, if different from permenantAddress.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="foreignTaxId" type="types:String50">
        <xs:annotation>
          <xs:documentation>
                        The foreign tax identifying number.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="foreignTaxIdType" type="types:String3">
        <xs:annotation>
          <xs:documentation>
                        The type of foreign tax identifying number (BN or SIN).  Only provided when dobVersion&gt;1.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="tin" type="types:FederalTaxIdentifier">
        <xs:annotation>
          <xs:documentation>
                        The federal tax id number if they have one.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="type" type="dobtypes:FederalTaxIdTypes">
        <xs:annotation>
          <xs:documentation>
                        The type of tax id provided, SSN or EIN.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CarrierInfo">
    <xs:annotation>
      <xs:documentation>
                The general identifying attributes of a carrier.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The last date and time any of the data in this section was updated.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="legalName" type="types:String155">
        <xs:annotation>
          <xs:documentation>
                        The legal name that this carrier operates under.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="dbaName" type="types:String155">
        <xs:annotation>
          <xs:documentation>
                        The Doing-Business-AS name of the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="dotNumber" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        The FMCSA DOT number assigned to this carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="docket" type="cwtypes:DocketType">
        <xs:annotation>
          <xs:documentation>
                        The FMCSA MC/MX/FF number assigned to this carrier.
                        <br />
                        DOT docket is made up of a prefix and a number.
                        Prefix is a 2-character String.
                        The number is a non-negative 32-bit integer. 
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="intrastate" type="cwtypes:IntrastateType">
        <xs:annotation>
          <xs:documentation>
                        The intrastate code given to this carrier.  Only provided if the carrier is only a 
                        intrastate carrier.
                        <br />
                        The intrastate type is made up of 
                        1) The state the carrier is authorized to operate within. A 2-character string.
                        2) The state's assigned operating code for this carrier. A 15-digit number.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="duns" type="dobtypes:DUNS">
        <xs:annotation>
          <xs:documentation>
                        The 9 digit duns number for this carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="scacCode" type="types:String4">
        <xs:annotation>
          <xs:documentation>
                        The SCAC codes assigned to this carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="website" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation>
                        Company website URL, max 256 characters.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="physicalAddress" type="cwtypes:Location">
        <xs:annotation>
          <xs:documentation>
                        The physical address and phone numbers of this company.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="mailingAddress" type="cwtypes:Location">
        <xs:annotation>
          <xs:documentation>
                        The mailing address and phone numbers of this company.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ContactInfo">
    <xs:annotation>
      <xs:documentation>
                The payment information for a carrier.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The last date and time any of the contacts were updated.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="contacts" type="dobtypes:Contact">
        <xs:annotation>
          <xs:documentation>
                        List of personnel and their roles in the company.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PaymentInfo">
    <xs:annotation>
      <xs:documentation>
                The payment information for a carrier.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The last date and time any of the data in this section was updated.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="remitToName" type="types:String155">
        <xs:annotation>
          <xs:documentation>
                        The name to make payments out to.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="remitToAddress" type="cwtypes:Location">
        <xs:annotation>
          <xs:documentation>
                        The address to send payments.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="isFactoringCompany" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not the remit-to name and address are for a factoring company.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="interestInQuickPay" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates interest in quick-pay plans.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="references" type="dobtypes:Reference">
        <xs:annotation>
          <xs:documentation>
                        Payment references that may be contacted.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Services">
    <xs:annotation>
      <xs:documentation>
                The services that the carrier provides and organizations that the carrier is a member.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The last date and time any of the data in this section was updated.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="companyType" type="dobtypes:CompanyTypes">
        <xs:annotation>
          <xs:documentation>
                        The type of company this is.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="yearFounded" type="types:Year">
        <xs:annotation>
          <xs:documentation>
                        The year the company was founded.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="womanOwned" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates if the carrier is owned by a woman.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="veteranOwned" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates if the carrier is owned by a military veteran.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="disabledVeteranOwned" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates if the carrier is owned by a disabled military veteran.  
                        Only provided when dobVersion&gt;1 is specified.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="minorityOwnership" type="dobtypes:MinorityOwnership">
        <xs:annotation>
          <xs:documentation>
                        Indicates if the carrier is owned by a minority.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="isSmartwayCarrier" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates if carrier is a Smartway carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="isHazmatCertified" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates if carrier is hazmat certified.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="isRadioActiveCertified" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates if carrier is certified to carry radioactive material.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="isAceCertified" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates if carrier is certified by ACE.
                        <br />
                        Boolean type.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="isCtpatMember" type="xs:boolean">

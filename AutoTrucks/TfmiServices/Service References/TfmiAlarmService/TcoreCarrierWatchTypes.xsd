<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:types="http://www.tcore.com/TcoreTypes.xsd" xmlns:cwtypes="http://www.tcore.com/TcoreCarrierWatchTypes.xsd" elementFormDefault="qualified" targetNamespace="http://www.tcore.com/TcoreCarrierWatchTypes.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="TcoreTypes.xsd" namespace="http://www.tcore.com/TcoreTypes.xsd" />
  <xs:annotation>
    <xs:documentation>
      <h2>Tcore Carrier Watch Types Schema</h2>
      <br />

            This schema defines a set of standard type definitions for general use by other TransCore CarrierWatch schema.  
            <br />
            Published:  Feburary, 2010<br />
            Version  :  1.0.0<br /><br /><i>Copyright (C) 2010, TransCore LP, All Rights Reserved.</i><br /><br /><br /></xs:documentation>
  </xs:annotation>
  <xs:complexType name="Header">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="tcoreId" type="xs:long">
        <xs:annotation>
          <xs:documentation>
                        TransCore's own carrier identifier
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="legalName" type="types:String155">
        <xs:annotation>
          <xs:documentation>
                        Best available legal business name. 
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="dbaName" type="types:String155">
        <xs:annotation>
          <xs:documentation>
                        Best available commonly used business name.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="dotNumber" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        DOT Identification Number of the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="docket" type="cwtypes:DocketType" />
      <xs:element minOccurs="0" maxOccurs="1" name="intrastate" type="cwtypes:IntrastateType" />
      <xs:element minOccurs="0" maxOccurs="1" name="duns" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        DUNS Number, when available.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="scac" type="types:String4">
        <xs:annotation>
          <xs:documentation>
                        When available, the Standard Carrier Alpha Code (SCAC) is a unique code used to identify transportation companies.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="physicalLocation" type="cwtypes:Location">
        <xs:annotation>
          <xs:documentation>
                        The best and last available physical address.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="mailingLocation" type="cwtypes:Location">
        <xs:annotation>
          <xs:documentation>
                        The best and last available mailing address.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DotProfile">
    <xs:annotation>
      <xs:documentation>
                DOT Profile of the carrier queried.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdateOfDotProfile" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The last date on which the DOT updated their profile for this carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="isActive" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not the carrier is still operating or not.  If not, 
                        no other data will be provided in this section.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="entityType" type="types:String30">
        <xs:annotation>
          <xs:documentation>
                        The DOT classification for the type of company this is.<br /><ul><li>Carrier</li><li>Shipper</li><li>Registrant</li><li>Carrier &amp; Shipper</li></ul>
                        Abnormal values may be:
                        <ul><li>Intermodal Equipment Provider</li><li>New Entrant Status</li></ul></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="operationType" type="types:String25">
        <xs:annotation>
          <xs:documentation>
                        Identifies the carrier as being engaged in interstate, intrastate hazardous 
                        material, or intrastate non-hazardous material transport activities.<br /><ul><li>Interstate</li><li>Intrastate Only (HM)</li><li>Intrastate Only (Non-HM)</li></ul></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="outOfInterstateServiceDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>
                        The date the carrier went out of Interstate service.  Only provided if the carrier is out of service.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="powerUnits" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        The number of power units operated by the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="drivers" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        The number of drivers employed and contracted by the carrier. 
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="mcs150Mileage">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="1" maxOccurs="1" name="mileageYear" type="types:NonNegativeInt">
              <xs:annotation>
                <xs:documentation>
                                    Mileage year from the MCS-150 Registration Form.  
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="1" maxOccurs="1" name="formDate" type="xs:date">
              <xs:annotation>
                <xs:documentation>
                                    Date from the MCS-150 Registration Form. 
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="1" maxOccurs="1" name="mileage" type="xs:long">
              <xs:annotation>
                <xs:documentation>
                                    Mileage from the MCS-150 Registration Form. 
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="commodities" type="cwtypes:CommodityType">
        <xs:annotation>
          <xs:documentation>
                        Commodities handled by the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="specialCommodities" type="types:String256">
        <xs:annotation>
          <xs:documentation>
                        Free form text containing a description of commodities not covered in 
                        the standard DOT commodity list.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DotInsuranceType">
    <xs:annotation>
      <xs:documentation>
                Describes a type of insurance carried by this carrier.
                An element will be included for each of the insurance types 
                carried by the carrier (possibly up to 5 types).<br /><br />
                Included only if the 'includeDotInsurance' flag is specified.<br /><br />
                Describes the mandated insurance coverage filed with the FMSCA. 
                This will include Primary BIPD insurance for all carriers 
                (Form BMC-91). It may include excess BIPD insurance if applicable 
                (Form BMC-91X). Common carriers are also required to file evidence 
                of cargo coverage (Form BMC-34). If the entity has brokerage 
                authority, this section will also include details of the broker 
                bond (Form BMC-84) or trust fund (Form BMC-85).<br /><ul><li>BIPD: Bodily Injury and Personal Damage, required for all carriers.</li><li>Cargo: Cargo coverage, required for common carriers.</li><li>Bond: Broker bond.</li><li>Trust: Trust fund.</li></ul>
                Brokers must provide evidence of a bond or trust fund. Either is 
                acceptable. Each element will only be present if the target 
                carrier also holds brokerage authority under the same MC (docket number).
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdateOfDotInsurance" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The last date on which the DOT's insurance records for this carrier were updated.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="insuranceRecord" type="cwtypes:InsuranceRecord">
        <xs:annotation>
          <xs:documentation>
                        The DOT insurance records provided by the DOT.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DocketType">
    <xs:annotation>
      <xs:documentation>
                DOT docket prefix and number. (MC, MX, or FF numbers);
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="prefix">
        <xs:annotation>
          <xs:documentation>
                        Docket Prefix.  Not avaliable in the first release.
                    </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="types:String2">
            <xs:enumeration value="MC" />
            <xs:enumeration value="MX" />
            <xs:enumeration value="FF" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="number" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Docket Number of the company.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="IntrastateType">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="intrastateState" type="types:StateProvince">
        <xs:annotation>
          <xs:documentation>
                        The state the carrier is authorized to operate within.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="intrastateCode" type="types:String15">
        <xs:annotation>
          <xs:documentation>
                        The state's assigned operating code for this carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DotAuthority">
    <xs:annotation>
      <xs:documentation>
                Provides DOT authority and insurance requirements.  
                Only included if the DOT Authority is selected for download and the carrier has a known docket number.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdateOfDotAuthority" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The last date on which the DOT updated their authority information for this carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="commonAuthority" type="cwtypes:AuthorityStatus">
        <xs:annotation>
          <xs:documentation>
                        Common authority for the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="pendingCommonAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not common authority is pending.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="revokedCommonAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not common authority may be revoked in the near future.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="contractAuthority" type="cwtypes:AuthorityStatus">
        <xs:annotation>
          <xs:documentation>
                        Contract authority for the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="pendingContractAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not contract authority is pending.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="revokedContractAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not contract authority may be revoked in the near future.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="brokerAuthority" type="cwtypes:AuthorityStatus">
        <xs:annotation>
          <xs:documentation>
                        Broker authority for the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="pendingBrokerAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not broker authority is pending.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="revokedBrokerAuthority" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not broker authority may be revoked in the near future.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="carriesFreight" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not the carrier is authorized for freight.<br /></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="carriesPassengers" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not the carrier is authorized to carry passengers. 
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="carriesHhg" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether or not the carrier is authorized to carry 
                        household goods. 
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="bipdRequired" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        The amount of BIPD coverage this carrier is required to have.<br /></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="cargoRequired" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether this carrier is required to file form BMC-34 showing 
                        evidence of cargo coverage.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="bondSuretyRequired" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether this carrier is required to file form BMC-84 or 85 
                        showing evidence of a broker bond or equivalent trust fund.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="bipdOnFile" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        The amount of BIPD coverage on file. Coverage details will be available 
                        in the active/pending insurance section.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="cargoOnFile" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether this carrier has filed form BMC-34 showing evidence of 
                        cargo coverage.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="bondOnFile" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
                        Indicates whether this carrier has also filed form BMC-84 or 85 showing 
                        evidence of a broker bond or trust fund.<br /><br />
                        If true, coverage details will be in the active/pending insurance 
                        section.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InsuranceRecord">
    <xs:annotation>
      <xs:documentation>
                DOT provided insurance data on the company.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="coverageType" type="cwtypes:InsuranceType">
        <xs:annotation>
          <xs:documentation>
                        BIPD, Cargo, Trust, or Bond
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="bipdClass" type="cwtypes:InsuranceClass">
        <xs:annotation>
          <xs:documentation>
                        If insuranceType = BIPD this element indicates whether the following 
                        coverage details are for primary coverage or excess coverage.<br /><br />
                        For other insurance types, this element will be omitted.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="formCode">
        <xs:annotation>
          <xs:documentation>
                        34 = Cargo<br />
                        91,91X = BIPD<br />
                        82 = BIPD (Surety Bond)<br />
                        83 = Cargo (Surety Bond)<br />
                        84 = Property Brokers Surety Bond<br />
                        85 = Property Brokers Trust Fund Agreement<br /></xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="types:String3">
            <xs:enumeration value="34" />
            <xs:enumeration value="82" />
            <xs:enumeration value="83" />
            <xs:enumeration value="84" />
            <xs:enumeration value="85" />
            <xs:enumeration value="91" />
            <xs:enumeration value="91X" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="policyNumber" type="types:String40">
        <xs:annotation>
          <xs:documentation>
                        The insurance policy number.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="coverageFrom" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Indicates the lower limit of the coverage. Usually zero except for 
                        Excess BIPD insurance. 
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="coverageTo" type="types:NonNegativeInt">
        <xs:annotation>
          <xs:documentation>
                        Indicates the upper limit of the coverage.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="effectiveDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>
                        Date this insurance policy became effective.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="canceledDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>
                        Date this insurance policy was canceled.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="insuranceCarrier">
        <xs:annotation>
          <xs:documentation>
                        The contact information for the insurance carrier.
                    </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="1" maxOccurs="1" name="id" type="types:NonNegativeInt">
              <xs:annotation>
                <xs:documentation>
                                    A unique identifier provided by Volpe.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="1" maxOccurs="1" name="companyName" type="types:String55">
              <xs:annotation>
                <xs:documentation>
                                    Insurance company name.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="contact" type="types:String55">
              <xs:annotation>
                <xs:documentation>
                                    Insurance company contact name.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="location" type="cwtypes:Location">
              <xs:annotation>
                <xs:documentation>
                                    Insurance company address.
                                    <br />
                                    Note that there are instances of incorrect registration data, in which 
                                    the postal code is not of the proper format for the designated country
                                    code.
                                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InsuranceCertificates">
    <xs:annotation>
      <xs:documentation>
                Describes a certification of insurance carried by the carrier.
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="lastUpdateOfInsuranceCertificates" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>
                        The last date on which the insurance certificate for this carrier was updated
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="maxCargoLimit" type="types:Currency">
        <xs:annotation>
          <xs:documentation>
                        The maximum coverage limit in the cargo insurance.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="coverage" type="cwtypes:InsuranceCertificate">
        <xs:annotation>
          <xs:documentation>
                        Describes cargo coverage carried by the carrier.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InsuranceCertificate">
    <xs:annotation>
      <xs:documentation>
                Describes an insurance certificate 
            </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="coverageCategory" type="types:String20">
        <xs:annotation>
          <xs:documentation>
                        The type of category this certificate falls under: CARGO, AUTO, GENERAL, WORKERS_COMP, or OTHER.  
                        OTHER may occur multiple times, but the others will occur only once.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="coverageType" type="types:String90">
        <xs:annotation>
          <xs:documentation>
                        The full name provided by the insurance company that describes the type of coverage.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="effectiveDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>
                        The date on which this policy went into or is going into effect.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="expirationDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>
                        The date on which this policy will expire. 
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="canceledDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>
                        The date on which this policy will be canceled.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="confidence">
        <xs:annotation>
          <xs:documentation>
                        High or Low.  
                        The confidence level that the TransCore will be informed promptly of any changes to this policy by the insurance carrier.
                    </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="types:String4">
            <xs:enumeration value="High" />
            <xs:enumeration value="Low" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="policyNumber" type="types:String90">
        <xs:annotation>
          <xs:documentation>
                        The policy number of the policy.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="agentName" type="types:String90">
        <xs:annotation>
          <xs:documentation>
                        Issuing insurance agent.
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="agentPhone" type="types:PhoneNumber">
        <xs:annotation>
          <xs:documentation>
                        Contact number of the insurance agent. 
                    </xs:documentation>
        </xs:annotation>
      </xs:element>
